!--      {{data1}}-->\r\n      <Table :columns=\"columns1\" :data=\"data1\"></Table>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"processVariables\",\r\n  data(){\r\n    return{\r\n      data1:[],\r\n      currentChoose:'',\r\n      selectRadioItem:null,\r\n      columns1: [\r\n        {\r\n          title: '单选', //复选框\r\n          key: 'flag',\r\n          width: 60,\r\n          'checkeds':true,//自定义属性，默认选中，必须展示的数据\r\n          render: (h, params) => {\r\n            let id = params.row.id;\r\n            let flag = false;\r\n            if (this.currentChoose === id) {\r\n              flag = true\r\n            } else {\r\n              flag = false\r\n            }\r\n            let self = this\r\n\r\n            // if(top.IVR_SetNode_Play_layui_open){\r\n            return h('Radio', {\r\n              props: {\r\n                value: flag\r\n              },\r\n              on: {\r\n                'on-change': () => {\r\n                  self.currentChoose = id;\r\n                  self.selectRadioItem=params.row;\r\n                  //self.selectRadioItem.content=self.rowPlay(params.row,'hide');\r\n                }\r\n              },\r\n            })\r\n            // }else {\r\n            //   return h('Checkbox', {\r\n            //     props: {\r\n            //       value: params.row.id\r\n            //     }\r\n            //   })\r\n            // }\r\n          }\r\n        },\r\n        {\r\n          title: \"编号\", //序号\r\n          key: 'id',\r\n          width: 60,\r\n          align: \"center\",\r\n        },\r\n        {\r\n          title: '变量名',\r\n          key: 'name',\r\n          editable: true,\r\n          render: (h, params) => {\r\n            return  this.setDataColumn(params,h,'name');\r\n          }\r\n        },\r\n        {\r\n          title: '类型',\r\n          key: 'type',\r\n          editable: true,\r\n          select:true,\r\n          option:[{key:'1',val:'布尔型'},{key:'2',val:'文本'},{key:'3',val:'长整型'},{key:'4',val:'短整型'},{key:'5',val:'浮点型'},{key:'6',val:'日期'}],\r\n          render:(h,params)=>{\r\n            return  this.setDataColumn(params,h,'type');\r\n          }\r\n        },{\r\n          title: '长度',\r\n          key: 'nLength',\r\n          editable: true,\r\n          render:(h,params)=>{\r\n            return  this.setDataColumn(params,h,'nLength');\r\n          }\r\n        },{\r\n          title: '值',\r\n          key: 'value',\r\n          editable: true,\r\n          render:(h,params)=>{\r\n            return  this.setDataColumn(params,h,'value');\r\n          }\r\n        },{\r\n          title: '描述',\r\n          key: 'describe',\r\n          editable: true,\r\n          render:(h,params)=>{\r\n            return  this.setDataColumn(params,h,'describe');\r\n          }\r\n        },\r\n        {\r\n          title: '操作',\r\n          key: 'type',\r\n          editable: true,\r\n          render: (h, params) => {\r\n            return h(\"div\", [\r\n              //删除\r\n              h('Tooltip',{props: {\r\n                  placement: 'top',\r\n                  transfer: true\r\n                }},[h(\r\n                  \"Icon\",\r\n                  {\r\n                    props: {\r\n                      type: \"ios-trash-outline\",\r\n                      size: \"\",\r\n                    },\r\n                    style: { marginRight: \"5px\",fontSize:'18px',cursor: 'pointer',display:params.row.tag==2?'block':'none'},\r\n                    on: {\r\n                      click: () => {\r\n                        this.rowDel(params.index,params.row);\r\n                      },\r\n                    },\r\n                  },\r\n                  this.$t(\"play\")\r\n              ),h('span', {\r\n                slot: 'content',\r\n                style: {\r\n                  whiteSpace: 'normal'\r\n                }\r\n              }, '删除')])\r\n\r\n            ]);\r\n          },\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  methods:{\r\n    init(){\r\n      this.data1=JSON.parse(JSON.stringify(window.wfGlobalVariables));\r\n    },\r\n    addTableList(){\r\n      var than=this;\r\n      than.data1.push({\r\n        id: than.data1.length+1,\r\n        name: \"\",\r\n        type: 0,\r\n        typeName:\"\",\r\n        nLength: 0,\r\n        describe : \"\",\r\n        value: \"\",\r\n        tag:2,\r\n        del:false\r\n      })\r\n    },\r\n    setDataColumn(params,h,name){\r\n      var str=null;\r\n      var than=this;\r\n      if (params.row.tag==2) {\r\n        if(params.column.select!=undefined){\r\n          return h('Select', {\r\n            props: {\r\n              value: params.row[name]\r\n            },\r\n            on: {\r\n              \"on-change\": event => {\r\n\r\n                params.row[name] = event;\r\n                than.data1[params.index][name] = event;\r\n              }\r\n            }\r\n          },params.column.option.map((item)=>{\r\n            return h('Option',{\r\n              props:{\r\n                value:item.key,\r\n                label:item.val\r\n              }\r\n            })\r\n          }));\r\n        }else {\r\n          return str= h('Input', {\r\n            props: {\r\n              type: \"text\",\r\n              value: params.row[name]\r\n            },\r\n\r\n            on: {\r\n              \"on-change\": event => {\r\n\r\n                params.row[name] = event.target.value;\r\n                than.data1[params.index][name] = event.target.value;\r\n              },\r\n\r\n            }\r\n          });\r\n        }\r\n      } else  {\r\n        if(params.column.select!=undefined){\r\n\r\n          return h(\"div\",params.column.option.map((item)=>{\r\n            if(item.key==params.row[name]){\r\n              return item.val\r\n            }\r\n          }));\r\n        }else {\r\n          return h(\"div\",params.row[name]);\r\n        }\r\n      }\r\n      return str;\r\n    },\r\n    rowDel(i,v){\r\n      console.log(v)\r\n      this.data1.splice(i,1);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.init();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.ui-table-warp{\r\n  height: 500px;\r\n  overflow: auto;\r\n}\r\n</style>"]}]}