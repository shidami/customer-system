A;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;;AAGA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"SetNode_Message.vue","sourceRoot":"src/views/my-components/IVR","sourcesContent":["<template>\r\n  <Row style=\"padding-top: 20px;background: #fff;\">\r\n    <Col span=\"22\" offset=\"1\">\r\n      <!--            {{formItem}}-->\r\n      <!--      <hr>-->\r\n      <!--      {{nextNodeIdArr}}-->\r\n      <Form class=\"ui-combination-form\" ref=\"formItem\" :model=\"formItem\" :rules=\"formItemValidate\" :label-width=\"160\">\r\n        <FormItem label=\"节点号\" prop=\"id\">\r\n          <Input v-model=\"formItem.id\" placeholder=\"请输入...\" disabled></Input>\r\n        </FormItem>\r\n        <FormItem label=\"节点名称\" prop=\"name\">\r\n          <Input v-model=\"formItem.name\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"重播次数\" prop=\"replayCount\">\r\n          <Input v-model=\"formItem.replayCount\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"播放语音\" prop=\"formItem.voiceList.describe\" class=\"ui-voiceList-item\">\r\n          <Row :gutter=\"16\" style=\"margin-bottom: 5px\">\r\n            <Col span=\"15\" prop=\"describe\" style=\"padding-left: 0;\">\r\n              <Input v-model=\"formItem.voiceList.describe\"  placeholder=\"请选择\" disabled></Input>\r\n            </Col>\r\n            <Col span=\"8\" style=\"text-align: left;padding-left: 0 !important;\">\r\n              <Button @click=\"selectVoice('voice')\" style=\"margin-left: 10px\">选择</Button>\r\n            </Col>\r\n          </Row>\r\n        </FormItem>\r\n        <FormItem label=\"结束提示音\" v-if=\"endRecordVoiceShow\" prop=\"formItem.endRecordVoice.describe\" class=\"ui-voiceList-item\">\r\n          <Row :gutter=\"16\" style=\"margin-bottom: 5px\">\r\n            <Col span=\"15\" style=\"padding-left: 0;\">\r\n              <Input v-model=\"formItem.endRecordVoice.describe\"  placeholder=\"请选择\" disabled></Input>\r\n            </Col>\r\n            <Col span=\"8\" style=\"text-align: left;padding-left: 0 !important;\">\r\n              <Button @click=\"selectVoice('endRecordVoice')\" style=\"margin-left: 10px\">选择</Button>\r\n            </Col>\r\n          </Row>\r\n        </FormItem>\r\n        <FormItem label=\"跳转节点\" prop=\"nextNodeId\">\r\n          <Select v-model=\"formItem.nextNodeId\">\r\n            <Option :value=\"item.value\" v-for=\"(item,i) in nextNodeIdList\" :key=\"i\">{{item.text}}</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"说明\" prop=\"describe\">\r\n          <Input type=\"textarea\" v-model=\"formItem.describe\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem>\r\n          <Button @click=\"btnOk\" type=\"primary\">确定</Button>\r\n          <Button @click=\"btnCancel\" style=\"margin-left: 8px\">取消</Button>\r\n        </FormItem>\r\n      </Form>\r\n    </Col>\r\n\r\n    <Modal\r\n        title=\"选择语音文件\"\r\n        v-model=\"voiceVisible\"\r\n        :closable=\"false\"\r\n        :mask-closable=\"false\"\r\n        :width=\"800\"\r\n    >\r\n      <voiceManage ref=\"getSelectData\"></voiceManage>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"voiceVisible = false\">{{\r\n            $t(\"cancel\")\r\n          }}</Button>\r\n        <Button\r\n            type=\"primary\"\r\n            @click=\"btnVoice\"\r\n        >{{ $t(\"save\") }}</Button\r\n        >\r\n      </div>\r\n    </Modal><!--选择语音文件-结束--->\r\n  </Row>\r\n</template>\r\n\r\n<script>\r\nimport voiceManage from \"@/views/sys/voice-manage/voiceManage\";\r\nimport processVariables from \"@/views/my-components/IVR/processVariables\";\r\nvar frame = parent;\r\nexport default {\r\n  name: \"SetNode_Message\",\r\n  components:{voiceManage,processVariables},\r\n  data(){\r\n    return{\r\n      openerid:WFCore.GetQuery(\"openerid\"),\r\n      stepid:WFCore.GetQuery(\"id\"),\r\n      voiceVisible:false,\r\n      nextNodeIdList:[{value:0,text:'无'}],\r\n      voiceType:'voice',\r\n      endRecordVoiceShow:false,\r\n      formItem:{\r\n        id:'',//节点号\r\n        name:'',//节点名称\r\n        replayCount:'',//重播次数\r\n        //添加播放内容\r\n        voiceList:{\r\n          id:'',\r\n          name:'',\r\n          type:'',\r\n          content:'',\r\n          describe:'',\r\n          tag:''\r\n        },\r\n        nextNodeId:0,//跳转节点\r\n        sort:'',//排序序号\r\n        describe:'',//说明\r\n      },\r\n      formItemValidate:{\r\n        name: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n      }\r\n    }\r\n  },\r\n  methods:{\r\n    init(){\r\n      this.initData();\r\n    },\r\n    initData(){\r\n      var than=this;\r\n      if (frame == null) return;\r\n      than.flowdata=top.wMyflowData;\r\n      than.step = than.flowdata.getNode(than.stepid);\r\n      if(than.flowdata.nodes.size()>0){\r\n        than.flowdata.nodes.each(function (nodeId,node,index){\r\n          if(than.step.checkNextNodeIsAllow(\"nextNodeId\",node.id)){\r\n            than.nextNodeIdList.push({value:node.id,text:[node.id]+'-' + node.getText()})\r\n          }\r\n        })\r\n      }\r\n\r\n      this.initStep(than.step);\r\n    },\r\n    initStep(step) {\r\n      var than=this;\r\n      if(step==null||step==undefined){\r\n        than.formItem.name='新流程';\r\n        return;\r\n      }\r\n      than.formItem.id=step.id;\r\n      than.formItem.name=step.getText();\r\n      than.formItem.replayCount=step.replayCount;\r\n      than.formItem.sort=step.sort;\r\n      than.formItem.describe=step.describe;\r\n\r\n      if(than.step.playVoice!=undefined){\r\n\r\n        //than.formItem.voiceList=[];\r\n        // for (var i = 0; i < step.playVoices.size(); i++) {\r\n        //objVoices.push(step.playVoices.get(i));\r\n        //than.addPlayVoice(step.playVoices.get(i));\r\n        debugger\r\n        //than.formItem.voiceList.push(step.playVoices.get(i))\r\n        than.formItem.voiceList.content=step.playVoice.content;\r\n        than.formItem.voiceList.describe=step.playVoice.describe;\r\n        than.formItem.voiceList.tag=step.playVoice.tag;\r\n        than.formItem.voiceList.id=step.playVoice.id;\r\n        than.formItem.voiceList.name=step.playVoice.name;\r\n        than.formItem.voiceList.type=step.playVoice.type;\r\n        //}\r\n\r\n      }else {\r\n        //than.addPlayVoice();\r\n      }\r\n\r\n      if(step.playVoice!=undefined){\r\n        than.formItem.voiceList={\r\n          id:step.playVoice.id,\r\n          name:step.playVoice.name,\r\n          type:step.playVoice.type,\r\n          content:step.playVoice.content,\r\n          describe:step.playVoice.describe,\r\n          tag:step.playVoice.tag\r\n        };\r\n      }\r\n      if(step.endRecordVoice!=undefined){\r\n        than.endRecordVoiceShow=true;\r\n        than.formItem.endRecordVoice={\r\n          id:step.playVoice.id,\r\n          name:step.playVoice.name,\r\n          type:step.playVoice.type,\r\n          content:step.playVoice.content,\r\n          describe:step.playVoice.describe,\r\n          tag:step.playVoice.tag\r\n        };\r\n      }\r\n      //跳转节点\r\n      than.ShowNodeResult(\"nextNodeId\",\"#base_NextNodeId\",1);\r\n      //跳转节点\r\n      //than.ShowNodeResult(\"nextNodeId\",\"#base_NextNodeId\",1);\r\n      // than.formItem.pressKeyList.forEach(function (v,i,arr){\r\n      //   if(v.id!=\"noPressKey\"){\r\n      //     than.ShowNodeResult(v.id,\"#base_NextNodeId\",0,i,v);\r\n      //   }else {\r\n      //     than.ShowNodeResult(v.id,\"#base_NextNodeId\",1,i,v);\r\n      //   }\r\n      // });\r\n      window.setTimeout(function (){\r\n        than.formItem.nextNodeId=than.nextNodeIdStr;\r\n      },500)\r\n    },\r\n    //给NodeResult控件赋值\r\n    ShowNodeResult(resultName,nextNodeCtlId,isDefault){\r\n      var than=this;\r\n      if(than.step.getNodeResult(resultName) != null && than.step.getNodeResult(resultName).nextNodeId > 0)\r\n      {\r\n        than.formItem[resultName]=parseInt(than.step.getNodeResult(resultName).nextNodeId);\r\n        than.nextNodeIdStr=parseInt(than.step.getNodeResult(resultName).nextNodeId);\r\n      }\r\n    },\r\n    //保存NodeResult控件所选下一节点\r\n    SaveNodeResult(resultName,nextNodeCtlId,describe,isDefault){\r\n\r\n      debugger\r\n      var than=this;\r\n      if(than.step.getNodeResult(resultName) != null){\r\n        if(than.formItem.nextNodeId != null && than.formItem.nextNodeId != \"\")\r\n        {\r\n          var nodeResult = than.step.getNodeResult(resultName).restore({\r\n            nextNodeId:isNaN(than.formItem[resultName]) ? 0 : parseInt(than.formItem[resultName]) || 0,\r\n            isDefault:isDefault || 0\r\n          });\r\n          if(nodeResult != null) nodeResult.drawNodeResult();\r\n        }\r\n        else\r\n        {\r\n          than.step.removeNodeResult(resultName);\r\n        }\r\n      }\r\n      else{\r\n        if(than.formItem[resultName] != null && than.formItem[resultName] != \"\")\r\n        {\r\n          var nodeResult = than.step.createNodeResult({\r\n            name:resultName,\r\n            nextNodeId:isNaN(than.formItem[resultName]) ? 0 : parseInt(than.formItem[resultName]) || 0,\r\n            isDefault:isDefault || 0,\r\n            describe:describe\r\n          });\r\n          if(nodeResult != null) nodeResult.drawNodeResult();\r\n        }\r\n      }\r\n    },\r\n    btnVoice(){\r\n      var voice=this.$refs.getSelectData.selectRadioItem;\r\n      var voicePublic=this.$refs.getSelectData.$refs.publicVoiceManage.selectRadioItem;\r\n      var obj=voice;\r\n      if(voicePublic.id==undefined){\r\n        obj=voice;\r\n        obj.tag=0;\r\n      }else {\r\n        obj=voicePublic;\r\n        obj.tag=1;\r\n      }\r\n      if(this.voiceType=='voice'){\r\n        this.formItem.voiceList.id=obj.id;\r\n        this.formItem.voiceList.name=\"voice\";\r\n        this.formItem.voiceList.type=1;\r\n        this.formItem.voiceList.content=obj.content;\r\n        this.formItem.voiceList.describe=obj.audio;\r\n        this.formItem.voiceList.tag=obj.tag;\r\n      }else {\r\n        this.formItem.endRecordVoice.id=obj.id;\r\n        this.formItem.endRecordVoice.name=\"voice\";\r\n        this.formItem.endRecordVoice.type=1;\r\n        this.formItem.endRecordVoice.content=obj.content;\r\n        this.formItem.endRecordVoice.describe=obj.audio;\r\n        this.formItem.endRecordVoice.tag=obj.tag;\r\n      }\r\n      this.voiceVisible=false;\r\n    },\r\n    btnOk(){\r\n      var than=this;\r\n      var isFormValid = new RoadUI.Validate().validateForm(\"form1\");\r\n      // if(isNaN($(\"#base_replayCount\").val()) || parseInt($(\"#base_replayCount\").val()) > 10)\r\n      // {\r\n      //   WFCore.DialogUtils.tipDialog(\"重播次数必须在1次到10次之间\");\r\n      //   return false;\r\n      // }\r\n      // if($(\"#base_collectVariable\").val() != \"\")\r\n      // {\r\n      //   var variableObj = step.ownerWorkflow.getVariable($(\"#base_collectVariable\").val());\r\n      //   if(variableObj == null)\r\n      //   {\r\n      //     WFCore.DialogUtils.tipDialog(\"变量名【\"+$(\"#base_collectVariable\").val()+\"】未在流程属性中定义\");\r\n      //     return false;\r\n      //   }\r\n      //   else if(variableObj.type.getIndex() != WFCore.Enums.VarType.String.getIndex())\r\n      //   {\r\n      //     WFCore.DialogUtils.tipDialog(\"变量名【\"+$(\"#base_collectVariable\").val()+\"】不是文本型变量\");\r\n      //     return false;\r\n      //   }\r\n      // }\r\n      // if(isNaN($(\"#base_maxCollectLength\").val()) || parseInt($(\"#base_maxCollectLength\").val()) < 0 || parseInt($(\"#base_maxCollectLength\").val()) > 32)\r\n      // {\r\n      //   WFCore.DialogUtils.tipDialog(\"最大收号位数必须在1位到32位之间\");\r\n      //   return false;\r\n      // }\r\n      // if(isNaN($(\"#base_collectTimeout\").val()) || parseInt($(\"#base_collectTimeout\").val()) < 0 || parseInt($(\"#base_collectTimeout\").val()) > 600)\r\n      // {\r\n      //   WFCore.DialogUtils.tipDialog(\"超时时间不允许小于0，最长600秒(10分钟)\");\r\n      //   return false;\r\n      // }\r\n      // if(isFormValid)\r\n      // {\r\n      //   than.confirm1();\r\n      // }\r\n      // else\r\n      // {\r\n      //   WFCore.DialogUtils.confirmDialog(\"系统提示\",\"节点配置不规范，是否忽略并保存？\",function(btn){\r\n      //     if(btn)\r\n      //     {\r\n      //       than.confirm1();\r\n      //     }\r\n      //   },\"忽略并保存\");\r\n      //   return false;\r\n      // }\r\n      this.$refs[\"formItem\"].validate((valid) => {\r\n        if (valid) {\r\n          this.confirm1();\r\n        }else {\r\n          this.$Message.error('请输入版本号');\r\n        }\r\n      })\r\n    },\r\n    confirm1() {\r\n      var than=this;\r\n      var ostep = {\r\n        text: {\r\n          text: than.formItem.name\r\n        },\r\n        sort: than.formItem.sort,\r\n        describe: than.formItem.describe,\r\n        replayCount: than.formItem.replayCount,\r\n      };\r\n\r\n\r\n      var trObj = than.formItem.voiceList;\r\n      if(trObj.content != \"\")\r\n      {\r\n        var curObj = {\r\n          id: WFCore.GetUUID(),\r\n          name: \"voice\",\r\n          type: 1,\r\n          content:trObj.content,\r\n          describe : trObj.describe,\r\n          tag: parseInt(trObj.tag) || 0\r\n        };\r\n\r\n        var voice = new parent.WFCore.Model.Voice();\r\n        voice.parseFromJson(curObj);\r\n        than.step.playVoice = voice;\r\n      }\r\n      else\r\n      {\r\n        than.step.playVoice = null;\r\n      }\r\n      //跳转节点\r\n      than.SaveNodeResult(\"nextNodeId\",\"#base_NextNodeId\",\"跳转节点\",1);\r\n      than.step.restore(ostep);\r\n      new RoadUI.Window().close();\r\n    },\r\n    btnCancel(){\r\n      new RoadUI.Window().close();\r\n    },\r\n    selectVoice(v){\r\n      this.voiceVisible=true\r\n      this.voiceType=v;\r\n    }\r\n  },\r\n  mounted() {\r\n    this.init();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"]}]}