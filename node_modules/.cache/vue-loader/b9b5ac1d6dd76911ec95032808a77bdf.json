{"remainingRequest":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\src\\views\\my-components\\IVR\\SetNode_Choice.vue?vue&type=style&index=0&id=51e823d8&scoped=true&lang=less&","dependencies":[{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\src\\views\\my-components\\IVR\\SetNode_Choice.vue","mtime":1614579763281},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\less-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["SetNode_Choice.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2aA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"SetNode_Choice.vue","sourceRoot":"src/views/my-components/IVR","sourcesContent":["<template>\r\n  <Row style=\"padding-top: 20px;background: #fff;\">\r\n    <Col span=\"22\" offset=\"1\">\r\n<!--      {{formItem}}-->\r\n<!--      <hr>-->\r\n<!--      {{nextNodeIdArr}}-->\r\n      <Form class=\"ui-combination-form\" ref=\"formItem\" :model=\"formItem\" :rules=\"formItemValidate\" :label-width=\"120\">\r\n        <FormItem label=\"节点号\" prop=\"id\">\r\n          <Input v-model=\"formItem.id\" placeholder=\"请输入...\" disabled></Input>\r\n        </FormItem>\r\n        <FormItem label=\"节点名称\" prop=\"name\">\r\n          <Input v-model=\"formItem.name\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"重播次数\" prop=\"replayCount\">\r\n          <Input v-model=\"formItem.replayCount\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"播放语音\" prop=\"id\" class=\"ui-voiceList-item\">\r\n          <Row :gutter=\"16\" style=\"margin-bottom: 5px\">\r\n            <Col span=\"15\" prop=\"describe\" style=\"padding-left: 0;\">\r\n              <Input v-model=\"formItem.voiceList.describe\"  placeholder=\"请选择\" disabled></Input>\r\n            </Col>\r\n            <Col span=\"8\" style=\"text-align: left;padding-left: 0 !important;\">\r\n              <Button @click=\"selVoiceListItem(i)\" style=\"margin-left: 10px\">选择</Button>\r\n            </Col>\r\n          </Row>\r\n        </FormItem>\r\n        <Row>\r\n          <Col span=\"12\" :gutter=\"16\" v-for=\"(item,i) in formItem.pressKeyList\" :key=\"i\" class=\"ui-nextNodeId-group\">\r\n            <FormItem :label=\"item.label\" prop=\"nextNodeId\"  v-if=\"i%2 ==0\" class=\"ui-nextNodeId-sel\">\r\n              <Select v-model=\"item.nextNodeId\">\r\n                <Option :value=\"item.value\" v-for=\"(item,i) in nextNodeIdList\" :key=\"i\">{{item.text}}</Option>\r\n              </Select>\r\n            </FormItem>\r\n            <Checkbox  v-if=\"i%2 ==0\" v-model=\"item.allowCreateLine\" class=\"ui-nextNodeId-cbx\">连线</Checkbox>\r\n\r\n\r\n            <FormItem  :label=\"item.label\" prop=\"nextNodeId\" v-if=\"i%2 !=0\" class=\"ui-nextNodeId-sel\">\r\n              <Select v-model=\"item.nextNodeId\">\r\n                <Option :value=\"item.value\" v-for=\"(item,i) in nextNodeIdList\" :key=\"i\">{{item.text}}</Option>\r\n              </Select>\r\n            </FormItem>\r\n            <Checkbox  v-if=\"i%2 !=0\" v-model=\"item.allowCreateLine\" class=\"ui-nextNodeId-cbx\">连线</Checkbox>\r\n          </Col>\r\n        </Row>\r\n        <FormItem label=\"说明\" prop=\"describe\">\r\n          <Input type=\"textarea\" v-model=\"formItem.describe\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem>\r\n          <Button @click=\"btnOk\" type=\"primary\">确定</Button>\r\n          <Button @click=\"btnCancel\" style=\"margin-left: 8px\">取消</Button>\r\n        </FormItem>\r\n      </Form>\r\n    </Col>\r\n\r\n    <Modal\r\n        title=\"选择语音文件\"\r\n        v-model=\"tableColumnsVisible\"\r\n        :closable=\"false\"\r\n        :mask-closable=\"false\"\r\n        :width=\"800\"\r\n    >\r\n      <voiceManage ref=\"getSelectData\"></voiceManage>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"tableColumnsVisible = false\">{{\r\n            $t(\"cancel\")\r\n          }}</Button>\r\n        <Button\r\n            type=\"primary\"\r\n            @click=\"btnVoice\"\r\n        >{{ $t(\"save\") }}</Button\r\n        >\r\n      </div>\r\n    </Modal><!--自定义表格展示的列-结束--->\r\n  </Row>\r\n</template>\r\n\r\n<script>\r\nimport voiceManage from \"@/views/sys/voice-manage/voiceManage\";\r\nvar frame = parent;\r\nexport default {\r\n  name: \"IVR_SetNode_Choice\",\r\n  components:{voiceManage},\r\n  data(){\r\n    return{\r\n      openerid:WFCore.GetQuery(\"openerid\"),\r\n      stepid:WFCore.GetQuery(\"id\"),\r\n      tableColumnsVisible:false,\r\n      formItem:{\r\n        id:'',//节点号\r\n        name:'',//节点名称\r\n        replayCount:'',//重播次数\r\n        //添加播放内容\r\n        voiceList:{\r\n          id:'',\r\n          name:'',\r\n          type:'',\r\n          content:'',\r\n          describe:'',\r\n          tag:''\r\n        },\r\n        //\r\n        pressKeyList:[\r\n          {id:'pressKey1',label:'按1',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKey2',label:'按2',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKey3',label:'按3',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKey4',label:'按4',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKey5',label:'按5',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKey6',label:'按6',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKey7',label:'按7',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKey8',label:'按8',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKey9',label:'按9',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKey0',label:'按0',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKeyStar',label:'按*',nextNodeId:0,allowCreateLine:true},\r\n          {id:'pressKeySharp',label:'按#',nextNodeId:0,allowCreateLine:true},\r\n          {id:'noPressKey',label:'默认跳转节点',nextNodeId:0,allowCreateLine:true},\r\n        ],\r\n        nextNodeId:0,//跳转节点\r\n        sort:'',//排序序号\r\n        describe:'',//说明\r\n      },\r\n      nextNodeIdList:[{value:0,text:'无'}],\r\n      nextNodeIdArr:[],\r\n      formItemValidate:{\r\n        name: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n      }\r\n    }\r\n  },\r\n  methods:{\r\n    init(){\r\n      this.initData();\r\n    },\r\n    initData(){\r\n      var than=this;\r\n      if (frame == null) return;\r\n      than.flowdata=top.wMyflowData;\r\n      than.step = than.flowdata.getNode(than.stepid);\r\n      if(than.flowdata.nodes.size()>0){\r\n        than.flowdata.nodes.each(function (nodeId,node,index){\r\n          if(than.step.checkNextNodeIsAllow(\"nextNodeId\",node.id)){\r\n            than.nextNodeIdList.push({value:node.id,text:[node.id]+'-' + node.getText()})\r\n          }\r\n        })\r\n      }\r\n\r\n      this.initStep(than.step);\r\n    },\r\n    initStep(step) {\r\n      var than=this;\r\n      if(step==null||step==undefined){\r\n        than.formItem.name='新流程';\r\n        return;\r\n      }\r\n      than.formItem.id=step.id;\r\n      than.formItem.name=step.getText();\r\n      than.formItem.replayCount=step.replayCount;\r\n      than.formItem.sort=step.sort;\r\n      than.formItem.describe=step.describe;\r\n\r\n\r\n      if(than.step.playVoice!=undefined){\r\n\r\n        //than.formItem.voiceList=[];\r\n       // for (var i = 0; i < step.playVoices.size(); i++) {\r\n          //objVoices.push(step.playVoices.get(i));\r\n          //than.addPlayVoice(step.playVoices.get(i));\r\n          debugger\r\n          //than.formItem.voiceList.push(step.playVoices.get(i))\r\n          than.formItem.voiceList.content=step.playVoice.content;\r\n          than.formItem.voiceList.describe=step.playVoice.describe;\r\n          than.formItem.voiceList.tag=step.playVoice.tag;\r\n          than.formItem.voiceList.id=step.playVoice.id;\r\n          than.formItem.voiceList.name=step.playVoice.name;\r\n          than.formItem.voiceList.type=step.playVoice.type;\r\n        //}\r\n\r\n      }else {\r\n        //than.addPlayVoice();\r\n      }\r\n\r\n      //跳转节点\r\n      //than.ShowNodeResult(\"nextNodeId\",\"#base_NextNodeId\",1);\r\n      than.formItem.pressKeyList.forEach(function (v,i,arr){\r\n        if(v.id!=\"noPressKey\"){\r\n          than.ShowNodeResult(v.id,\"#base_NextNodeId\",0,i,v);\r\n        }else {\r\n          than.ShowNodeResult(v.id,\"#base_NextNodeId\",1,i,v);\r\n        }\r\n      });\r\n      window.setTimeout(function (){\r\n        than.formItem.pressKeyList=than.nextNodeIdArr;\r\n      },500)\r\n    },\r\n    btnOk(){\r\n      var than=this;\r\n      this.$refs[\"formItem\"].validate((valid) => {\r\n        if (valid) {\r\n          than.confirm1();\r\n        }\r\n      })\r\n\r\n      // var isFormValid = new RoadUI.Validate().validateForm(\"form1\");\r\n      // if(isNaN($(\"#base_replayCount\").val()) || parseInt($(\"#base_replayCount\").val()) > 10)\r\n      // {\r\n      //   WFCore.DialogUtils.tipDialog(\"重播次数至少1次，不允许超过10次\");\r\n      //   return false;\r\n      // }\r\n      // if(isFormValid)\r\n      // {\r\n      //   than.confirm1();\r\n      // }\r\n      // else\r\n      // {\r\n      //   WFCore.DialogUtils.confirmDialog(\"系统提示\",\"节点配置不规范，是否忽略并保存？\",function(btn){\r\n      //     if(btn)\r\n      //     {\r\n      //       than.confirm1();\r\n      //     }\r\n      //   },\"忽略并保存\");\r\n      //   return false;\r\n      // }\r\n    },\r\n    confirm1() {\r\n      var than=this;\r\n      // var ostep = {\r\n      //   text: {\r\n      //     text: $(\"#base_Name\").val() || \"\"\r\n      //   },\r\n      //   sort: isNaN($(\"#base_Sort\").val()) ? 0 : parseInt($(\"#base_Sort\").val()) || 0,\r\n      //   describe: $(\"#base_Describe\").val() || \"\",\r\n      //   replayCount: isNaN($(\"#base_replayCount\").val()) ? 1 : parseInt($(\"#base_replayCount\").val()) || 1\r\n      //\r\n      // };\r\n      var ostep={\r\n        text:{\r\n          text:than.formItem.name\r\n        },\r\n        sort:than.formItem.sort,\r\n        describe:than.formItem.describe,\r\n        replayCount:than.formItem.replayCount\r\n      }\r\n\r\n      debugger\r\n      var trObj = than.formItem.voiceList;\r\n      if(trObj.content != \"\")\r\n      {\r\n        // var curObj = {\r\n        //   id: WFCore.GetUUID(),\r\n        //   name: \"voice\",\r\n        //   type: 1,\r\n        //   content:trObj.find(\"input[name='voiceFilePath']\").val() || \"\",\r\n        //   describe : trObj.find(\"input[name='voiceFile']\").val(),\r\n        //   tag: parseInt(trObj.find(\"input[name='voiceFile']\").attr(\"tag\")) || 0\r\n        // };\r\n        var playType = trObj.type;\r\n        var curObj = {\r\n          id: trObj.id,\r\n          name: playType == 1 ? \"voice\":\"TTS\",\r\n          type: playType,\r\n          content:playType == 1 ? trObj.content : trObj.content,\r\n          describe : playType == 1 ? trObj.describe : \"\",\r\n          tag:playType == 1 ? trObj.content : 2\r\n        };\r\n\r\n        var voice = new parent.WFCore.Model.Voice();\r\n        voice.parseFromJson(curObj);\r\n        than.step.playVoice = voice;\r\n      }\r\n      else\r\n      {\r\n        than.step.playVoice = null;\r\n      }\r\n      than.formItem.pressKeyList.forEach(function (v,i,arr){\r\n        if(v.id!=\"noPressKey\"){\r\n          than.SaveNodeResult(v.id,\"#pressKey1\",v.label,0,v);\r\n        }else {\r\n          than.SaveNodeResult(v.id,\"#pressKey1\",v.label,1,v);\r\n        }\r\n      });\r\n      // //按1\r\n      // than.SaveNodeResult(\"pressKey1\",\"#pressKey1\",\"按1\",0);\r\n      // //按2\r\n      // than.SaveNodeResult(\"pressKey2\",\"#pressKey2\",\"按2\",0);\r\n      // //按3\r\n      // than.SaveNodeResult(\"pressKey3\",\"#pressKey3\",\"按3\",0);\r\n      // //按4\r\n      // than.SaveNodeResult(\"pressKey4\",\"#pressKey4\",\"按4\",0);\r\n      // //按5\r\n      // than.SaveNodeResult(\"pressKey5\",\"#pressKey5\",\"按5\",0);\r\n      // //按6\r\n      // than.SaveNodeResult(\"pressKey6\",\"#pressKey6\",\"按6\",0);\r\n      // //按7\r\n      // than.SaveNodeResult(\"pressKey7\",\"#pressKey7\",\"按7\",0);\r\n      // //按8\r\n      // than.SaveNodeResult(\"pressKey8\",\"#pressKey8\",\"按8\",0);\r\n      // //按9\r\n      // than.SaveNodeResult(\"pressKey9\",\"#pressKey9\",\"按9\",0);\r\n      // //按0\r\n      // than.SaveNodeResult(\"pressKey0\",\"#pressKey0\",\"按0\",0);\r\n      // //按*\r\n      // than.SaveNodeResult(\"pressKeyStar\",\"#pressKeyStar\",\"按*\",0);\r\n      // //按#\r\n      // than.SaveNodeResult(\"pressKeySharp\",\"#pressKeySharp\",\"按#\",0);\r\n      // //未按键\r\n      // than.SaveNodeResult(\"noPressKey\",\"#noPressKey\",\"默认跳转节点\",1);\r\n      //step.playVoices = playVoices;\r\n      than.step.restore(ostep);\r\n      new RoadUI.Window().close();\r\n    },\r\n    btnCancel(){\r\n      new RoadUI.Window().close();\r\n    },\r\n    //给NodeResult控件赋值\r\n    ShowNodeResult(resultName,nextNodeCtlId,isDefault,_index,v) {\r\n      var than=this,obj={\r\n        id:v.id,\r\n        label:v.label,\r\n        nextNodeId:0,\r\n        allowCreateLine:true\r\n      };\r\n      if(v.id=='nextNodeId'){\r\n        debugger\r\n        console.log(v);\r\n      }\r\n\r\n\r\n    var nodeResult = than.step.getNodeResult(v.id);\r\n    if(nodeResult != null && nodeResult.nextNodeId > 0)\r\n    {\r\n      //var flowdata = frame.myflow1.GetMyFlowData();\r\n      var flowdata = than.flowdata;\r\n      var node = flowdata.getNode(nodeResult.nextNodeId);\r\n      if(node != null)\r\n      {\r\n        obj.nextNodeId=nodeResult.nextNodeId;\r\n        //$(nextNodeCtlId).val(nodeResult.nextNodeId+\"\");\r\n      }\r\n      obj.allowCreateLine=nodeResult.allowCreateLine==true?nodeResult.allowCreateLine:false;\r\n      // if(nodeResult.allowCreateLine)\r\n      // {\r\n      //  // $(nextNodeCtlId+\"_allowCreateLine\").attr(\"checked\",\"checked\");\r\n      // }\r\n      // else\r\n      // {\r\n      //   $(nextNodeCtlId+\"_allowCreateLine\").removeAttr(\"checked\");\r\n      // }\r\n    }\r\n\r\n      than.nextNodeIdArr.push(obj);\r\n    isDefault = isDefault != undefined && isDefault != null ? isDefault:0;\r\n    $(nextNodeCtlId).attr(\"isDefault\",isDefault);\r\n  },\r\n    //保存NodeResult控件所选下一节点\r\n    SaveNodeResult(resultName,nextNodeCtlId,describe,isDefault,v) {\r\n      var than=this;\r\n      // var allowCreateLine = false;\r\n      // if($(nextNodeCtlId+\"_allowCreateLine\").is(\":checked\"))\r\n      // {\r\n      //   allowCreateLine = true;\r\n      // }\r\n      var allowCreateLine = v.allowCreateLine;\r\n      if(than.step.getNodeResult(resultName) != null)\r\n      {\r\n        if(v.nextNodeId != null && v.nextNodeId != \"\")\r\n        {\r\n          var nodeResult = than.step.getNodeResult(resultName).restore({\r\n            nextNodeId:isNaN(v.nextNodeId) ? 0 : parseInt(v.nextNodeId) || 0,\r\n            isDefault:isDefault || 0,\r\n            allowCreateLine:allowCreateLine\r\n          });\r\n          if(nodeResult != null) nodeResult.drawNodeResult();\r\n        }\r\n        else\r\n        {\r\n          than.step.removeNodeResult(resultName);\r\n        }\r\n      }\r\n      else\r\n      {\r\n        if(v.nextNodeId != null && v.nextNodeId != \"\")\r\n        {\r\n          var nodeResult = than.step.createNodeResult({\r\n            name:resultName,\r\n            nextNodeId:isNaN(v.nextNodeId) ? 0 : parseInt(v.nextNodeId) || 0,\r\n            isDefault:isDefault || 0,\r\n            describe:describe,\r\n            allowCreateLine:allowCreateLine\r\n          });\r\n          if(nodeResult != null) nodeResult.drawNodeResult();\r\n        }\r\n      }\r\n    },\r\n    selVoiceListItem(i){\r\n      this.selIndex=i;\r\n      this.tableColumnsVisible=true;\r\n    },\r\n    btnVoice(){\r\n      var voice=this.$refs.getSelectData.selectRadioItem;\r\n      var voicePublic=this.$refs.getSelectData.$refs.publicVoiceManage.selectRadioItem;\r\n      var obj=voice;\r\n      if(voicePublic.id==undefined){\r\n        obj=voice;\r\n        obj.tag=0;\r\n      }else {\r\n        obj=voicePublic;\r\n        obj.tag=1;\r\n      }\r\n      this.formItem.voiceList.id=obj.id;\r\n      this.formItem.voiceList.name=\"voice\";\r\n      this.formItem.voiceList.type=1;\r\n      this.formItem.voiceList.content=obj.content;\r\n      this.formItem.voiceList.describe=obj.audio;\r\n      this.formItem.voiceList.tag=obj.tag;\r\n      this.tableColumnsVisible=false;\r\n\r\n    }\r\n  },\r\n  mounted() {\r\n    this.init();\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.ui-nextNodeId-group{\r\n  .ui-nextNodeId-sel{\r\n    width: calc(100% - 55px);display: inline-block;\r\n  }\r\n  .ui-nextNodeId-cbx{\r\n    display: inline-block;\r\n    width: 50px;\r\n    //overflow: hidden;\r\n    height: 25px;\r\n    margin: 7px 0 auto 5px;\r\n    vertical-align: middle;\r\n  }\r\n}\r\n\r\n</style>"]}]}