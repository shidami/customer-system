\" v-model=\"visible\" width=\"1350\" draggable>\r\n      <div :style=\"{ maxHeight: maxHeight }\" class=\"drawer-content\">\r\n        <Tabs v-model=\"activeName\" :animated=\"false\" @on-click=\"changeMenu\">\r\n          <TabPane label=\"租户信息\" name=\"租户信息\"></TabPane>\r\n          <TabPane label=\"部门管理\" name=\"部门管理\"></TabPane>\r\n          <TabPane label=\"用户管理\" name=\"用户管理\"></TabPane>\r\n          <TabPane label=\"角色管理\" name=\"角色管理\"></TabPane>\r\n          <TabPane label=\"中继资源\" name=\"中继资源\"></TabPane>\r\n          <TabPane label=\"分机号\" name=\"分机号\"></TabPane>\r\n        </Tabs>\r\n        <div style=\"padding: 8px 15px; width: 100%\">\r\n          <div>\r\n            <div v-show=\"currMenu == '租户信息'\">\r\n              <Card>\r\n              <div class=\"tenant-title\">\r\n                <div class=\"info-title\">基本信息</div>\r\n                <img\r\n                  :src=\"form.logo\"\r\n                  style=\"\r\n                    float: right;\r\n                    right: 30px;\r\n                    position: absolute;\r\n                    max-height: 40px;\r\n                    max-width: 200px;\r\n                  \"\r\n                />\r\n              </div>\r\n              <Form label-colon>\r\n                <Row :gutter=\"32\">\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"租户ID\">\r\n                      {{ form.id }}\r\n                      <Tooltip\r\n                        trigger=\"hover\"\r\n                        placement=\"right\"\r\n                        content=\"租户已禁用\"\r\n                      >\r\n                        <Icon\r\n                          v-show=\"form.status == -1\"\r\n                          type=\"md-lock\"\r\n                          size=\"18\"\r\n                          style=\"margin-left: 10px; cursor: pointer\"\r\n                        />\r\n                      </Tooltip>\r\n                    </FormItem>\r\n                  </Col>\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"租户名称\" prop=\"title\">\r\n                      {{ form.title }}\r\n                    </FormItem>\r\n                  </Col>\r\n                </Row>\r\n              </Form>\r\n              <Form ref=\"form\" :model=\"form\" label-position=\"top\">\r\n                <Row :gutter=\"32\">\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"租户编码\" prop=\"code\">\r\n                      {{ form.code }}\r\n                    </FormItem>\r\n                  </Col>\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"到期时间\" prop=\"experience_date\">\r\n                      {{ form.experience_date }}\r\n                    </FormItem>\r\n                  </Col>\r\n                </Row>\r\n                <Row :gutter=\"32\">\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"联系电话\" prop=\"mobile\">\r\n                      {{ form.mobile }}\r\n                    </FormItem>\r\n                  </Col>\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"邮箱\" prop=\"email\">\r\n                      {{ form.email }}\r\n                    </FormItem>\r\n                  </Col>\r\n                </Row>\r\n                <Row :gutter=\"32\">\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"正式租户\" prop=\"type\">\r\n                      <i-switch\r\n                        size=\"large\"\r\n                        v-model=\"form.type\"\r\n                        :true-value=\"1\"\r\n                        :false-value=\"0\"\r\n                        :disabled=\"true\"\r\n                      >\r\n                        <span slot=\"open\">是</span>\r\n                        <span slot=\"close\">否</span>\r\n                      </i-switch>\r\n                    </FormItem>\r\n                  </Col>\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"租户状态\" prop=\"status\">\r\n                      <i-switch\r\n                        size=\"large\"\r\n                        v-model=\"form.status\"\r\n                        :true-value=\"0\"\r\n                        :false-value=\"-1\"\r\n                        :disabled=\"true\"\r\n                      >\r\n                        <span slot=\"open\">启用</span>\r\n                        <span slot=\"close\">禁用</span>\r\n                      </i-switch>\r\n                    </FormItem>\r\n                  </Col>\r\n                </Row>\r\n                <Row>\r\n                  <Col span=\"24\">\r\n                    <FormItem label=\"简介\">\r\n                      {{ form.description }}\r\n                    </FormItem>\r\n                  </Col>\r\n                </Row>\r\n                <Divider />\r\n                <p class=\"info-header\">资源信息</p>\r\n                <Row :gutter=\"32\">\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"主CTI\" prop=\"cti_master\">\r\n                      {{ form.cti_master }}\r\n                    </FormItem>\r\n                  </Col>\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"备CTI\" prop=\"cti_slave\">\r\n                      {{ form.cti_slave }}\r\n                    </FormItem>\r\n                  </Col>\r\n                </Row>\r\n                <Row :gutter=\"32\">\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"最大通道数\" prop=\"max_channel_num\">\r\n                      <Tooltip\r\n                        trigger=\"hover\"\r\n                        placement=\"right\"\r\n                        content=\"支持的最大并发数\"\r\n                      >\r\n                        {{ form.max_channel_num }}\r\n                      </Tooltip>\r\n                    </FormItem>\r\n                  </Col>\r\n                  <Col span=\"12\">\r\n                    <FormItem label=\"分机数量\" prop=\"ext_num\">\r\n                      {{ form.ext_num }}\r\n                    </FormItem>\r\n                  </Col>\r\n                </Row>\r\n              </Form>\r\n              </Card>\r\n            </div>\r\n            <div v-show=\"currMenu == '部门管理'\" class=\"safe\">\r\n              <department v-model=\"visible\" />\r\n            </div>\r\n            <div v-show=\"currMenu == '分机号'\" class=\"safe\">\r\n              <ext v-model=\"visible\" :tenantId=\"form.id\" />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </Drawer>\r\n  </div>\r\n</template>\r\n \r\n<script>\r\nimport {\r\n  addTenant,\r\n  editTenant,\r\n  getAllCtiList,\r\n  getCTIGroupById,\r\n} from \"@/api/index\";\r\n\r\nimport { validateInteger } from \"@/libs/validate\";\r\nimport uploadPicInput from \"@/views/my-components/xboot/upload-pic-input\";\r\nimport department from \"../department-manage/departmentManage.vue\";\r\nimport ext from \"../ext-number/extNumber-Tenant.vue\";\r\nexport default {\r\n  name: \"tenant\",\r\n  components: {\r\n    uploadPicInput,\r\n    department,\r\n    ext,\r\n  },\r\n  props: {\r\n    value: {\r\n      type: Boolean,\r\n      default: false,\r\n    },\r\n    data: {\r\n      type: Object,\r\n    },\r\n    type: {\r\n      type: String,\r\n      default: \"0\",\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      currMenu: \"租户信息\",\r\n      activeName: \"租户信息\",\r\n      visible: this.value,\r\n      title: \"\",\r\n      submitLoading: false,\r\n      maxHeight: 510,\r\n      form: {},\r\n      formValidate: {\r\n        code: [\r\n          { required: true, message: \"租户编号不能为空\", trigger: \"blur\" },\r\n        ],\r\n        title: [\r\n          { required: true, message: \"租户名称不能为空\", trigger: \"blur\" },\r\n        ],\r\n        email: [{ type: \"email\", message: \"邮箱格式不正确\" }],\r\n        cti_master: [\r\n          { required: true, message: \"CTI不能为空\", trigger: \"blur\" },\r\n        ],\r\n        max_channel_num: [\r\n          {\r\n            type: \"integer\",\r\n            required: true,\r\n            message: \"通道数不能为空\",\r\n            trigger: \"blur\",\r\n          },\r\n          //{ type: \"number\", validator: validateInteger, trigger: \"blur\" },\r\n        ],\r\n        ext_num: [\r\n          {\r\n            type: \"integer\",\r\n            required: true,\r\n            message: \"分机数量不能为空\",\r\n            trigger: \"blur\",\r\n          },\r\n          //{ validator: validateInteger, trigger: \"blur\" },\r\n        ],\r\n        ctiList: [],\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.getAllCTI();\r\n    },\r\n    changeMenu(v) {\r\n      this.currMenu = v;\r\n    },\r\n    getAllCTI() {\r\n      getAllCtiList().then((res) => {\r\n        if (res.success) {\r\n          var result = res.result;\r\n          this.ctiList = result;\r\n        }\r\n      });\r\n    },\r\n    changeCTI(value) {\r\n      getCTIGroupById({ id: value }).then((res) => {\r\n        if (res.success && res.result != undefined) {\r\n          for (let index = 0; index < res.result.length; index++) {\r\n            if (res.result[index].id != value) {\r\n              this.form.cti_slave = res.result[index].id;\r\n              break;\r\n            } else this.form.cti_slave = null;\r\n          }\r\n        }\r\n      });\r\n    },\r\n    submit() {\r\n      this.$refs.form.validate((valid) => {\r\n        if (valid) {\r\n          if (typeof this.form.experience_date == \"object\") {\r\n            this.form.experience_date = this.format(\r\n              this.form.experience_date,\r\n              \"yyyy-MM-dd\"\r\n            );\r\n          }\r\n          if (this.form.cti_slave == undefined) this.form.cti_slave = -1;\r\n          if (this.type == \"1\") {\r\n            // 编辑\r\n            this.submitLoading = true;\r\n            editTenant(this.form).then((res) => {\r\n              this.submitLoading = false;\r\n              if (res.success) {\r\n                this.$Message.success(\"操作成功\");\r\n                this.$emit(\"on-submit\", true);\r\n                this.visible = false;\r\n              }\r\n            });\r\n          } else {\r\n            // 添加\r\n            this.submitLoading = true;\r\n            addTenant(this.form).then((res) => {\r\n              this.submitLoading = false;\r\n              if (res.success) {\r\n                this.$Message.success(\"操作成功\");\r\n                this.$emit(\"on-submit\", true);\r\n                this.visible = false;\r\n              }\r\n            });\r\n          }\r\n        }\r\n      });\r\n    },\r\n    changeExperienceDate(v, d) {\r\n      this.form.experience_date = v;\r\n    },\r\n    setCurrentValue(value) {\r\n      if (value === this.visible) {\r\n        return;\r\n      }\r\n      if (this.type == \"1\") {\r\n        this.title = \"编辑租户\";\r\n        this.maxHeight =\r\n          Number(document.documentElement.clientHeight - 121) + \"px\";\r\n      } else if (this.type == \"2\") {\r\n        this.title = \"添加租户\";\r\n        this.maxHeight =\r\n          Number(document.documentElement.clientHeight - 121) + \"px\";\r\n      } else {\r\n        this.title = \"租户详情\";\r\n        this.maxHeight = \"100%\";\r\n      }\r\n      // 清空数据\r\n      this.$refs.form.resetFields();\r\n      if (this.type == \"0\" || this.type == \"1\") {\r\n        // 回显数据\r\n        let data = this.data;\r\n        // 回显\r\n        this.form = data;\r\n      } else {\r\n        // 添加\r\n        this.form = {\r\n          max_channel_num: 30,\r\n          ext_num: 10,\r\n          status: 0,\r\n          type: 0,\r\n        };\r\n      }\r\n      this.visible = value;\r\n    },\r\n  },\r\n  watch: {\r\n    value(val) {\r\n      this.setCurrentValue(val);\r\n    },\r\n    visible(value) {\r\n      this.$emit(\"input\", value);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.init();\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"less\">\r\n@import \"../../../styles/table-common.less\";\r\n.drawer-content {\r\n  overflow: auto;\r\n}\r\n.drawer-content::-webkit-scrollbar {\r\n  display: none;\r\n}\r\n.tenant-title {\r\n  display: flex;\r\n  align-items: center;\r\n  margin-bottom: 16px;\r\n  .info-title {\r\n    font-size: 16px;\r\n    color: rgba(0, 0, 0, 0.85);\r\n    display: block;\r\n    margin-right: 16px;\r\n  }\r\n}\r\n.info-header {\r\n  font-size: 16px;\r\n  color: rgba(0, 0, 0, 0.85);\r\n  display: block;\r\n  margin-bottom: 16px;\r\n}\r\n</style>\r\n\r\n"]}]}