\r\n                    <Input\r\n                      type=\"text\"\r\n                      v-model=\"searchForm.title\"\r\n                      placeholder=\"请输入\"\r\n                      clearable\r\n                      style=\"width: 200px\"\r\n                    />\r\n                  </Form-item>\r\n                  <Form-item label=\"状态\" prop=\"status\">\r\n                    <Select\r\n                      v-model=\"searchForm.status\"\r\n                      placeholder=\"请选择\"\r\n                      clearable\r\n                      style=\"width: 200px\"\r\n                    >\r\n                      <Option value=\"0\">正常</Option>\r\n                      <Option value=\"-1\">禁用</Option>\r\n                    </Select>\r\n                  </Form-item>\r\n                  <Form-item style=\"margin-left:-35px;\" class=\"br\">\r\n                    <Button @click=\"handleSearch\" type=\"primary\" icon=\"ios-search\">搜索</Button>\r\n                    <Button @click=\"handleReset\">重置</Button>\r\n                  </Form-item>\r\n                </Form>\r\n              </Row> -->\r\n              <div class=\"own-space-right\">\r\n                <Row class=\"operation ui-operation\">\r\n                  <div\r\n                    class=\"ui-operation-left\"\r\n                    v-show=\"openTip && selectCount > 0\"\r\n                  >\r\n                    <Row class=\"ui-alert-warp\">\r\n                      <Alert>\r\n                        已选择\r\n                        <span class=\"select-count\">{{ selectCount }}</span> 项\r\n                        <a class=\"select-clear\" @click=\"clearSelectAll\">清空</a>\r\n                      </Alert>\r\n                    </Row>\r\n                    <Button @click=\"delAll\" icon=\"md-trash\">批量移除</Button>\r\n                    <Dropdown @on-click=\"handleDropdownLevle\">\r\n                      <Button>\r\n                        切换接续等级\r\n                        <Icon type=\"md-arrow-dropdown\" />\r\n                      </Button>\r\n                      <DropdownMenu slot=\"list\">\r\n                        <DropdownItem name=\"1\">1级</DropdownItem>\r\n                        <DropdownItem name=\"2\">2级</DropdownItem>\r\n                        <DropdownItem name=\"3\">3级</DropdownItem>\r\n                        <DropdownItem name=\"4\">4级</DropdownItem>\r\n                        <DropdownItem name=\"5\">5级</DropdownItem>\r\n                        <DropdownItem name=\"6\">6级</DropdownItem>\r\n                        <DropdownItem name=\"7\">7级</DropdownItem>\r\n                        <DropdownItem name=\"8\">8级</DropdownItem>\r\n                        <DropdownItem name=\"9\">9级</DropdownItem>\r\n                      </DropdownMenu>\r\n                    </Dropdown>\r\n                  </div>\r\n                  <div class=\"ui-operation-right\" v-show=\"selectCount == 0\">\r\n                    <user-choose\r\n                      text=\"选择坐席\"\r\n                      @on-change=\"handleSelectUser\"\r\n                      @click=\"submitUser\"\r\n                      :agentGroupId=\"agentGroupId\"\r\n                      ref=\"user\"\r\n                      style=\"display: inline-block\"\r\n                    ></user-choose>\r\n                  </div>\r\n\r\n                  <!-- <Button @click=\"getDataList\" icon=\"md-refresh\">刷新数据</Button>\r\n                  <Button type=\"dashed\" @click=\"openSearch=!openSearch\">{{openSearch ? \"关闭搜索\" : \"开启搜索\"}}</Button>\r\n                  <Button type=\"dashed\" @click=\"openTip=!openTip\">{{openTip ? \"关闭提示\" : \"开启提示\"}}</Button> -->\r\n                </Row>\r\n\r\n                <Row>\r\n                  <Table\r\n                    :loading=\"loading\"\r\n                    :columns=\"columns\"\r\n                    :data=\"data\"\r\n                    sortable=\"custom\"\r\n                    @on-sort-change=\"changeSort\"\r\n                    @on-selection-change=\"showSelect\"\r\n                    @on-row-click=\"showDetail\"\r\n                    ref=\"table\"\r\n                  ></Table>\r\n                </Row>\r\n                <Row type=\"flex\" justify=\"end\" class=\"page\">\r\n                  <Page\r\n                    :current=\"searchForm.pageNumber\"\r\n                    :total=\"total\"\r\n                    :page-size=\"searchForm.pageSize\"\r\n                    @on-change=\"changePage\"\r\n                    @on-page-size-change=\"changePageSize\"\r\n                    :page-size-opts=\"[10, 20, 50]\"\r\n                    size=\"small\"\r\n                    show-total\r\n                    show-elevator\r\n                    show-sizer\r\n                  ></Page>\r\n                </Row>\r\n              </div>\r\n            </Col>\r\n          </div>\r\n        </div>\r\n      </Row>\r\n    </Card>\r\n\r\n    <Modal\r\n      :title=\"modalTitle\"\r\n      v-model=\"agentGroupModalVisible\"\r\n      :mask-closable=\"false\"\r\n      :width=\"500\"\r\n    >\r\n      <Form\r\n        ref=\"agentGroupForm\"\r\n        :model=\"agentGroupForm\"\r\n        :label-width=\"100\"\r\n        :rules=\"agentGroupFormValidate\"\r\n      >\r\n        <FormItem label=\"技能组名称\" prop=\"title\">\r\n          <Input v-model=\"agentGroupForm.title\" />\r\n        </FormItem>\r\n        <FormItem label=\"弹屏链接\" prop=\"linkId\">\r\n          <Input v-model=\"agentGroupForm.linkId\" />\r\n        </FormItem>\r\n        <FormItem label=\"预测外呼速率\" prop=\"outCallRate\">\r\n          <Input v-model=\"agentGroupForm.outCallRate\" /><span\r\n            >(1.0~10.0之间)</span\r\n          >\r\n        </FormItem>\r\n        <FormItem label=\"是否有效\" prop=\"enabled\">\r\n          <i-switch\r\n            size=\"large\"\r\n            v-model=\"agentGroupForm.enabled\"\r\n            :true-value=\"1\"\r\n            :false-value=\"0\"\r\n          >\r\n            <span slot=\"open\">是</span>\r\n            <span slot=\"close\">否</span>\r\n          </i-switch>\r\n        </FormItem>\r\n        <FormItem label=\"备注\" prop=\"remark\">\r\n          <Input type=\"textarea\" v-model=\"agentGroupForm.remark\" :rows=\"4\" />\r\n        </FormItem>\r\n      </Form>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"cancelAgentGroup\">取消</Button>\r\n        <Button\r\n          type=\"primary\"\r\n          :loading=\"submitLoading\"\r\n          @click=\"submitAgentGroup\"\r\n          >提交</Button\r\n        >\r\n      </div>\r\n    </Modal>\r\n\r\n    <!-- <Modal :title=\"userTitle\" v-model=\"userModalVisible\" :mask-closable=\"false\" :width=\"500\">\r\n      <Form ref=\"userForm\" :model=\"userForm\" :label-width=\"100\" :rules=\"userFormValidate\">\r\n        <FormItem label=\"坐席名称\" prop=\"nickname\">\r\n          <Input v-model=\"userForm.nickname\"  />\r\n        </FormItem>\r\n        <FormItem label=\"所属技能组\" prop=\"groupId\">\r\n          <Select v-model=\"userForm.groupId\" placeholder=\"请选择\">\r\n            <Option v-for=\"(item, index) in treeData\" :key=\"index\" :value=\"item.id\" >{{ item.title }}</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"坐席分机\" prop=\"extCode\">\r\n          <Select v-model=\"userForm.extCode\" placeholder=\"请选择\">\r\n            <Option value=\"1\">固定分机</Option>\r\n            <Option value=\"2\">坐席自选</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"分机号码\" prop=\"extNumber\">\r\n          <Select v-model=\"userForm.extNumber\" placeholder=\"请选择\">\r\n             <Option v-for=\"(item, index) in queryCtiListItems\" :key=\"index\" :value=\"item.id\" >{{ item.ctiName }}</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"接续等级\" prop=\"continuationLevel\">\r\n          <Select v-model=\"userForm.continuationLevel\" placeholder=\"请选择\">\r\n            <Option value=\"1\">1级</Option>\r\n            <Option value=\"2\">2级</Option>\r\n            <Option value=\"3\">3级</Option>\r\n            <Option value=\"4\">4级</Option>\r\n            <Option value=\"5\">5级</Option>\r\n            <Option value=\"6\">6级</Option>\r\n            <Option value=\"7\">7级</Option>\r\n            <Option value=\"8\">8级</Option>\r\n            <Option value=\"9\">9级</Option>\r\n          </Select>\r\n        </FormItem>\r\n       \r\n      </Form>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"cancelAgentGroup\">取消</Button>\r\n        <Button type=\"primary\" :loading=\"submitLoading\" @click=\"submitAgentGroup\">提交</Button>\r\n      </div>\r\n    </Modal> -->\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  getAllAgentGroupList,\r\n  deleteAgentGroup,\r\n  editAgentGroup,\r\n  addAgentGroup,\r\n  getAgentUserList,\r\n  deleteAgentUserIds,\r\n  updatefindAgentUser,\r\n  submitUserList,\r\n  getAllDictList,\r\n  editBlackListGroups,\r\n} from \"@/api/index\";\r\nimport userChoose from \"@/views/sys/agent-group/user-choose\";\r\nexport default {\r\n  name: \"agent-group\",\r\n  components: {\r\n    userChoose,\r\n  },\r\n  data() {\r\n    return {\r\n      openSearch: true,\r\n      openTip: true,\r\n      treeLoading: false, // 树加载状态\r\n      maxHeight: \"500px\",\r\n      loading: false, // 表格加载状态\r\n      editTitle: \"\", // 编辑节点名称\r\n      searchKey: \"\", // 搜索树\r\n      expand: true,\r\n      span: 18,\r\n      expandIcon: \"ios-arrow-back\",\r\n      selectNode: {},\r\n      treeData: [], // 树数据\r\n      selectCount: 0, // 多选计数\r\n      selectList: [], // 多选数据\r\n      selectUsers: [],\r\n      agentGroupId: \"\",\r\n      searchForm: {\r\n        // 搜索框对应data对象\r\n        pageNumber: 1, // 当前页数\r\n        pageSize: 10, // 页面大小\r\n        sort: \"create_time\", // 默认排序字段\r\n        order: \"asc\", // 默认排序方式\r\n      },\r\n      modalType: 0, // 添加或编辑标识\r\n      agentGroupModalVisible: false, // 添加或编辑显示\r\n      //agentGroupFormValidate: false,\r\n      modalTitle: \"\", // 添加或编辑标题\r\n      userTitle: \"编辑坐席\",\r\n      userModalVisible: false,\r\n      userFormValidate: false,\r\n      userForm: {},\r\n      agentGroupForm: {\r\n        title: \"\",\r\n        linkId: \"\",\r\n        outCallRate: \"\",\r\n        remark: \"\",\r\n      },\r\n      agentGroupFormValidate: {\r\n        title: [\r\n          { required: true, message: \"技能名称不能为空\", trigger: \"blur\" },\r\n        ],\r\n      },\r\n      columns: [\r\n        // 表头\r\n        {\r\n          type: \"selection\",\r\n          width: 60,\r\n          align: \"center\",\r\n        },\r\n        {\r\n          type: \"index\",\r\n          width: 60,\r\n          align: \"center\",\r\n        },\r\n        {\r\n          title: \"姓名\",\r\n          key: \"nickname\",\r\n          minWidth: 130,\r\n          sortable: true,\r\n        },\r\n        {\r\n          title: \"坐席分机\",\r\n          key: \"extMode\",\r\n          minWidth: 130,\r\n          sortable: true,\r\n        },\r\n        {\r\n          title: \"分机号码\",\r\n          key: \"extNumber\",\r\n          minWidth: 130,\r\n          sortable: true,\r\n        },\r\n        {\r\n          title: \"接续等级\",\r\n          key: \"continuationLevel\",\r\n          minWidth: 130,\r\n          sortable: true,\r\n        },\r\n        {\r\n          title: \"操作\",\r\n          key: \"action\",\r\n          width: 70,\r\n          render: (h, params) => {\r\n            return h(\"div\", [\r\n              h(\r\n                \"Tooltip\",\r\n                {\r\n                  props: {\r\n                    placement: \"top\",\r\n                    transfer: true,\r\n                  },\r\n                },\r\n                [\r\n                  h(\r\n                    \"Icon\",\r\n                    {\r\n                      props: {\r\n                        type: \"ios-trash-outline\",\r\n                        size: \"\",\r\n                      },\r\n                      style: {\r\n                        marginRight: \"5px\",\r\n                        fontSize: \"18px\",\r\n                        cursor: \"pointer\",\r\n                      },\r\n                      on: {\r\n                        click: () => {\r\n                          this.remove(params.row);\r\n                        },\r\n                      },\r\n                    },\r\n                    this.$t(\"play\")\r\n                  ),\r\n                  h(\r\n                    \"span\",\r\n                    {\r\n                      slot: \"content\",\r\n                      style: {\r\n                        whiteSpace: \"normal\",\r\n                      },\r\n                    },\r\n                    \"移除\"\r\n                  ),\r\n                ]\r\n              ),\r\n            ]);\r\n          },\r\n        },\r\n      ],\r\n      submitLoading: false, // 添加或编辑提交状态\r\n      data: [], //表单数据\r\n      total: 0, // 表单数据总数\r\n    };\r\n  },\r\n  methods: {\r\n    init() {\r\n      // 获取树数据\r\n      this.getDataList();\r\n    },\r\n\r\n    getDataList() {\r\n      this.treeLoading = true;\r\n      getAllAgentGroupList().then((res) => {\r\n        this.treeLoading = false;\r\n        if (res.success) {\r\n          this.treeData = res.result;\r\n          if (res.result.length > 0) {\r\n            this.selectNode.id = res.result[0].id;\r\n            this.agentGroupId = res.result[0].id;\r\n            this.searchForm.agentGroupId = res.result[0].id;\r\n            this.getDataAgentUserList();\r\n          }\r\n        }\r\n      });\r\n    },\r\n    getDataAgentUserList() {\r\n      if (this.selectNode.id) {\r\n        this.searchForm.agentGroupId = this.selectNode.id;\r\n      } else {\r\n        this.data = [];\r\n        this.total = 0;\r\n        this.selectCount = 0;\r\n        return;\r\n      }\r\n      this.loading = true;\r\n      getAgentUserList(this.searchForm).then((res) => {\r\n        this.loading = false;\r\n        if (res.success) {\r\n          this.data = res.result.list;\r\n          this.total = res.result.total;\r\n          this.selectCount = 0;\r\n        }\r\n      });\r\n    },\r\n    selectTree(v) {\r\n      if (v.length > 0) {\r\n        this.$refs.agentGroupForm.resetFields();\r\n        // 转换null为\"\"\r\n        for (let attr in v[0]) {\r\n          if (v[0][attr] == null) {\r\n            v[0][attr] = \"\";\r\n          }\r\n        }\r\n        let str = JSON.stringify(v[0]);\r\n        let data = JSON.parse(str);\r\n        this.selectNode = data;\r\n        this.editTitle = data.title;\r\n        // 重新加载表\r\n        this.searchForm.pageNumber = 1;\r\n        this.searchForm.pageSize = 10;\r\n        this.getDataAgentUserList();\r\n        this.agentGroupId = this.selectNode.id;\r\n      } else {\r\n        this.cancelEdit();\r\n      }\r\n    },\r\n    cancelEdit() {\r\n      let data = this.$refs.tree.getSelectedNodes()[0];\r\n      if (data) {\r\n        data.selected = false;\r\n      }\r\n\r\n      // 取消选择后获取全部数据\r\n      this.selectNode = {};\r\n      this.editTitle = \"\";\r\n      this.getDataAgentUserList();\r\n    },\r\n    changeSelect(v) {\r\n      this.selectCount = v.length;\r\n      this.selectList = v;\r\n    },\r\n    changeExpand() {\r\n      this.expand = !this.expand;\r\n      if (this.expand) {\r\n        this.expandIcon = \"ios-arrow-back\";\r\n        this.span = 18;\r\n      } else {\r\n        this.expandIcon = \"ios-arrow-forward\";\r\n        this.span = 23;\r\n      }\r\n    },\r\n    changePage(v) {\r\n      this.searchForm.pageNumber = v;\r\n      //this.getDataList();\r\n      this.clearSelectAll();\r\n    },\r\n    changePageSize(v) {\r\n      this.searchForm.pageSize = v;\r\n      //this.getDataList();\r\n    },\r\n    handleSearch() {\r\n      this.searchForm.pageNumber = 1;\r\n      this.searchForm.pageSize = 10;\r\n      this.getDataList();\r\n    },\r\n    handleReset() {\r\n      this.$refs.searchForm.resetFields();\r\n      this.searchForm.pageNumber = 1;\r\n      this.searchForm.pageSize = 10;\r\n      // 重新加载数据\r\n      this.getDataList();\r\n    },\r\n    changeSort(e) {\r\n      this.searchForm.sort = e.key;\r\n      this.searchForm.order = e.order;\r\n      if (e.order == \"normal\") {\r\n        this.searchForm.order = \"\";\r\n      }\r\n      this.getDataList();\r\n    },\r\n    showSelect(e) {\r\n      this.selectList = e;\r\n      this.selectCount = e.length;\r\n      if (this.selectCount > 0) {\r\n        this.openTip = true;\r\n      } else {\r\n        this.openTip = false;\r\n      }\r\n    },\r\n    clearSelectAll() {\r\n      this.$refs.table.selectAll(false);\r\n    },\r\n    refreshDict() {\r\n      this.getDataList();\r\n      this.getDataAgentUserList();\r\n      this.selectNode = {};\r\n      this.editTitle = \"\";\r\n    },\r\n    handleDropdown(name) {\r\n      if (name == \"editDcit\") {\r\n        if (!this.selectNode.id) {\r\n          this.$Message.warning(\"您还未选择要编辑的技能组\");\r\n          return;\r\n        }\r\n        this.editAgentGroup();\r\n      } else if (name == \"delDcit\") {\r\n        this.delAgentGroupId();\r\n      } else if (name == \"refreshDcit\") {\r\n        this.refreshDict();\r\n      }\r\n    },\r\n    addAgentGroup() {\r\n      this.showAgentGroup = true;\r\n      this.modalType = 0;\r\n      this.$refs.agentGroupForm.resetFields();\r\n      this.agentGroupForm = { enabled: 1 };\r\n      this.agentGroupModalVisible = true;\r\n      this.modalTitle = \"新增技能组\";\r\n    },\r\n    editAgentGroup() {\r\n      var v = this.selectNode;\r\n      // 转换null为\"\"\r\n      for (let attr in v) {\r\n        if (v[attr] == null) {\r\n          delete v[attr];\r\n        }\r\n        if (attr == \"enabled\") {\r\n          v[attr] = parseInt(v[attr]);\r\n        }\r\n      }\r\n      this.modalType = 1;\r\n      this.agentGroupModalVisible = true;\r\n      this.modalTitle = \"编辑技能组\";\r\n      this.$refs.agentGroupForm.resetFields();\r\n      le