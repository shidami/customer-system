{"remainingRequest":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\src\\views\\my-components\\IVR\\SetNode_SeatGroup.vue?vue&type=style&index=0&id=3c7a6493&scoped=true&lang=less&","dependencies":[{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\src\\views\\my-components\\IVR\\SetNode_SeatGroup.vue","mtime":1614579763281},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\less-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["SetNode_SeatGroup.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiZA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA","file":"SetNode_SeatGroup.vue","sourceRoot":"src/views/my-components/IVR","sourcesContent":["<template>\r\n  <Row style=\"padding-top: 20px;background: #fff;\">\r\n    <Col span=\"22\" offset=\"1\">\r\n<!--            {{formItem}}-->\r\n      <!--      {{nextNodeIdStr}}-->\r\n      <Form class=\"ui-combination-form\" ref=\"formItem\" :model=\"formItem\" :rules=\"formItemValidate\" :label-width=\"120\">\r\n        <FormItem label=\"节点号\" prop=\"id\">\r\n          <Input v-model=\"formItem.id\" placeholder=\"请输入...\" disabled></Input>\r\n        </FormItem>\r\n        <FormItem label=\"节点名称\" prop=\"name\">\r\n          <Input v-model=\"formItem.name\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"技能\" prop=\"groupCategoryId\" class=\"ui-voiceList-item\">\r\n          <Select v-model=\"formItem.groupCategoryId\">\r\n            <Option :value=\"item.id\" v-for=\"(item,i) in agentGroupData\" :key=\"i\">{{item.title}}</Option>\r\n          </Select>\r\n        </FormItem>\r\n\r\n        <FormItem label=\"排队策略\" prop=\"queueMode\" class=\"ui-voiceList-item\">\r\n          <RadioGroup v-model=\"formItem.queueMode\">\r\n            <Radio label=\"1\">轮询</Radio>\r\n            <Radio label=\"2\">最少空闲</Radio>\r\n            <Radio label=\"3\">最长空闲</Radio>\r\n          </RadioGroup>\r\n        </FormItem>\r\n        <FormItem label=\"排队超时时间\" prop=\"queueTimeout\">\r\n          <Input v-model=\"formItem.queueTimeout\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"等待语音\" prop=\"voiceFile\">\r\n          <Row :gutter=\"16\" style=\"margin-bottom: 5px\">\r\n            <Col span=\"15\" prop=\"describe\" style=\"padding-left: 0;\">\r\n              <Input v-model=\"formItem.voiceFile.describe\"  placeholder=\"请输入\" disabled></Input>\r\n            </Col>\r\n            <Col span=\"4\" style=\"text-align: left;padding-left: 0 !important;\">\r\n              <Button @click=\"voiceFileVisible=true\" style=\"margin-left: 10px\">选择</Button>\r\n            </Col>\r\n          </Row>\r\n        </FormItem>\r\n\r\n        <FormItem label=\"无坐席跳转节点\" prop=\"noSeatNextNodeId\" v-show=\"formItem.noSeatMode==0\">\r\n          <Select v-model=\"formItem.noSeatNextNodeId\">\r\n            <Option :value=\"item.value\" v-for=\"(item,i) in nextNodeIdList\" :key=\"i\">{{item.text}}</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"无坐席继续等待\" prop=\"noSeatMode\">\r\n          <RadioGroup v-model=\"formItem.noSeatMode\">\r\n            <Radio label=\"1\">开启</Radio>\r\n            <Radio label=\"0\">关闭</Radio>\r\n          </RadioGroup>\r\n        </FormItem>\r\n        <FormItem label=\"队列满跳转节点\" prop=\"fullQueueNextNodeId\" v-show=\"formItem.fullQueueMode==0\">\r\n          <Select v-model=\"formItem.fullQueueNextNodeId\">\r\n            <Option :value=\"item.value\" v-for=\"(item,i) in nextNodeIdList\" :key=\"i\">{{item.text}}</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"队列满继续等待\" prop=\"fullQueueMode\">\r\n          <RadioGroup v-model=\"formItem.fullQueueMode\">\r\n            <Radio label=\"1\">开启</Radio>\r\n            <Radio label=\"0\">关闭</Radio>\r\n          </RadioGroup>\r\n        </FormItem>\r\n        <FormItem label=\"超时跳转节点\" prop=\"queueTimeoutNextNodeId\" v-show=\"formItem.queueTimeoutMode==0\">\r\n          <Select v-model=\"formItem.queueTimeoutNextNodeId\">\r\n            <Option :value=\"item.value\" v-for=\"(item,i) in nextNodeIdList\" :key=\"i\">{{item.text}}</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"队列满继续等待\" prop=\"queueTimeoutMode\">\r\n          <RadioGroup v-model=\"formItem.queueTimeoutMode\">\r\n            <Radio label=\"1\">开启</Radio>\r\n            <Radio label=\"0\">关闭</Radio>\r\n          </RadioGroup>\r\n        </FormItem>\r\n        <FormItem label=\"说明\" prop=\"describe\">\r\n          <Input type=\"textarea\" v-model=\"formItem.describe\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem>\r\n          <Button @click=\"btnOk\" type=\"primary\">确定</Button>\r\n          <Button @click=\"btnCancel\" style=\"margin-left: 8px\">取消</Button>\r\n        </FormItem>\r\n      </Form>\r\n    </Col>\r\n    <Modal\r\n        title=\"选择语音文件\"\r\n        v-model=\"voiceFileVisible\"\r\n        :closable=\"false\"\r\n        :mask-closable=\"false\"\r\n        :width=\"800\"\r\n    >\r\n      <voiceManage ref=\"getSelectData\"></voiceManage>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"voiceFileVisible = false\">{{\r\n            $t(\"cancel\")\r\n          }}</Button>\r\n        <Button\r\n            type=\"primary\"\r\n            @click=\"btnVoice\"\r\n        >{{ $t(\"save\") }}</Button\r\n        >\r\n      </div> </Modal\r\n    ><!--自定义表格展示的列-结束--->\r\n  </Row>\r\n</template>\r\n\r\n<script>\r\nimport voiceManage from \"@/views/sys/voice-manage/voiceManage\";\r\nimport {getAllAgentGroupList} from \"@/api/index\";\r\nvar frame = parent;\r\nexport default {\r\n  name: \"SetNode_SeatGroup\",\r\n  components:{voiceManage},\r\n  data(){\r\n    return{\r\n      layParent:true,\r\n      voiceFileVisible:false,\r\n      variableMappingVisible:false,\r\n      processVariablesVisible:false,\r\n      selProcessVariablesType:'str',\r\n      selProcessVariablesIndex:0,\r\n      openerid:WFCore.GetQuery(\"openerid\"),\r\n      stepid:WFCore.GetQuery(\"id\"),\r\n      flowdata:top.wMyflowData,\r\n      nextNodeIdList:[{value:0,text:'无'}],\r\n      selIndex:0,\r\n      step:'',\r\n      formItem:{\r\n        id:'',//节点号\r\n        name:'',//节点名称\r\n        groupCategoryId:'',//技能\r\n        queueMode:'1',//排队策略\r\n        queueTimeout:'',//排队超时时间\r\n        voiceFile:{\r\n          content:'',\r\n          describe:'',\r\n          tag:'',\r\n        },//等待语音\r\n        noSeatNextNodeId:'',//无坐席跳转节点\r\n        noSeatMode:'2',//无坐席继续等待\r\n        fullQueueNextNodeId:'',//队列满跳转节点\r\n        fullQueueMode:'2',//队列满继续等待\r\n        queueTimeoutNextNodeId:'',//超时跳转节点\r\n        queueTimeoutMode:'2',//超时继续等待\r\n        sort:'',//排序序号\r\n        describe:'',//说明\r\n      },\r\n      inBlacklistNextNodeIdStr:0,//黑名单跳转节点-绑定\r\n      notInBlacklistNextNodeIdStr:0,//非黑名单跳转节点-绑定\r\n      formItemValidate:{\r\n        name: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n        httpURL: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n        postParams: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n        returnVariable: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }]\r\n      },\r\n      agentGroupData:[],\r\n    }\r\n  },\r\n  methods:{\r\n    init(){\r\n      this.initData();\r\n      this.getAllAgentGroupDataList();\r\n    },\r\n    initData(){\r\n      var than=this;\r\n      if (frame == null) return;\r\n      than.flowdata=top.wMyflowData;\r\n      than.step = than.flowdata.getNode(than.stepid);\r\n      if(than.flowdata.nodes.size()>0){\r\n        than.flowdata.nodes.each(function (nodeId,node,index){\r\n          if(than.step.checkNextNodeIsAllow(\"nextNodeId\",node.id)){\r\n            than.nextNodeIdList.push({value:node.id,text:[node.id]+'-' + node.getText()})\r\n          }\r\n        })\r\n      }\r\n\r\n      this.initStep(than.step);\r\n    },\r\n    getAllAgentGroupDataList(){\r\n      getAllAgentGroupList().then(res=>{\r\n        if (res.success) {\r\n          this.agentGroupData = res.result;\r\n        }\r\n      })\r\n    },\r\n    initStep(step) {\r\n      var than=this;\r\n      if(step==null||step==undefined){\r\n        than.formItem.name='新流程';\r\n        return;\r\n      }\r\n      debugger\r\n      than.formItem.id=step.id;\r\n      than.formItem.name=step.getText();\r\n      than.formItem.groupCategoryId=step.seatGroupCategoryId;\r\n      than.formItem.queueMode=step.queueMode.toString();\r\n      than.formItem.noSeatMode=step.noSeatMode.toString();\r\n      than.formItem.fullQueueMode=step.fullQueueMode.toString()?step.fullQueueMode.toString():'2';\r\n      than.formItem.queueTimeout=step.queueTimeout;\r\n      than.formItem.queueTimeoutMode=step.queueTimeoutMode.toString();\r\n\r\n      than.formItem.sort=step.sort;\r\n      than.formItem.describe=step.describe;\r\n\r\n      if(step.waitVoice != null){\r\n        than.formItem.voiceFile={\r\n          id: step.waitVoice.id,\r\n          name: step.waitVoice.name,\r\n          type: step.waitVoice.type,\r\n          content:step.waitVoice.content,\r\n          describe : step.waitVoice.describe,\r\n          tag: step.waitVoice.tag\r\n        }\r\n      }\r\n\r\n      if(step.noSeatMode == 0) {\r\n        than.ShowNodeResult(\"noSeatNextNodeId\",\"#noSeatNextNodeId\");\r\n      }\r\n      if(step.fullQueueMode == 0) {\r\n        than.ShowNodeResult(\"fullQueueNextNodeId\",\"#fullQueueNextNodeId\");\r\n      }\r\n      if(step.queueTimeoutMode == 0) {\r\n        than.ShowNodeResult(\"queueTimeoutNextNodeId\",\"#queueTimeoutNextNodeId\",1);\r\n      }\r\n      //跳转节点\r\n      //than.ShowNodeResult(\"nextNodeId\",\"#nextNodeId\",1);\r\n      //跳转节点\r\n      // than.ShowNodeResult(\"nextNodeId\",\"#base_NextNodeId\",0);\r\n      window.setTimeout(function (){\r\n        than.formItem.nextNodeId=than.nextNodeIdStr;\r\n      },100)\r\n    },\r\n    btnOk(){\r\n      var than=this;\r\n      var isFormValid = new RoadUI.Validate().validateForm(\"form1\");\r\n      if(this.formItem.replayCount > 10||this.formItem.replayCount <1)\r\n      {\r\n        WFCore.DialogUtils.tipDialog(\"重播次数在1-10之间\",2,7);\r\n        return false;\r\n      }\r\n      this.$refs[\"formItem\"].validate((valid) => {\r\n        if (valid) {\r\n          than.confirm1();\r\n        }\r\n      })\r\n    },\r\n    btnCancel(){\r\n      new RoadUI.Window().close();\r\n    },\r\n    confirm1() {\r\n      var than=this;\r\n\r\n      var ostep={\r\n        text:{\r\n          text:than.formItem.name\r\n        },\r\n        sort:than.formItem.sort,\r\n        describe:than.formItem.describe,\r\n        groupCategoryId:than.formItem.groupCategoryId,\r\n        queueMode:than.formItem.queueMode,\r\n        queueTimeout:than.formItem.queueTimeout,\r\n        seatGroupCategoryId:than.formItem.groupCategoryId,\r\n\r\n        postParams:than.formItem.postParams,\r\n        returnVariable:than.formItem.returnVariable\r\n      }\r\n\r\n      var trObj = than.formItem.voiceFile;\r\n      if(trObj.voiceFile != \"\")\r\n      {\r\n        var curObj = {\r\n          id: WFCore.GetUUID(),\r\n          name: \"voice\",\r\n          type: 1,\r\n          content:trObj.content || \"\",\r\n          describe : trObj.describe,\r\n          tag: parseInt(trObj.tag) || 0\r\n        };\r\n\r\n        var voice = new parent.WFCore.Model.Voice();\r\n        voice.parseFromJson(curObj);\r\n        than.step.waitVoice = voice;\r\n      }\r\n      else\r\n      {\r\n        than.step.waitVoice = null;\r\n      }\r\n\r\n      //非黑名单跳转节点\r\n      ///than.SaveNodeResult(\"nextNodeId\",\"#nextNodeId\",\"跳转节点\",1);\r\n\r\n      // var playVoices = new parent.WFCore.List();\r\n      // var varIndex = 1;\r\n      ostep.noSeatMode = than.formItem.noSeatMode;\r\n      than.SaveNodeResult(\"noSeatNextNodeId\",\"#noSeatNextNodeId\",\"无坐席跳转节点\");\r\n      ostep.fullQueueMode = than.formItem.fullQueueMode;\r\n      than.SaveNodeResult(\"fullQueueNextNodeId\",\"#fullQueueNextNodeId\",\"队列满跳转节点\");\r\n      ostep.queueTimeoutMode = than.formItem.queueTimeoutMode;\r\n      than.SaveNodeResult(\"queueTimeoutNextNodeId\",\"#queueTimeoutNextNodeId\",\"超时跳转节点\",1);\r\n\r\n      window.setTimeout(function (){\r\n        than.step.restore(ostep);\r\n        new RoadUI.Window().close();\r\n      },100)\r\n    },\r\n    //给NodeResult控件赋值\r\n    ShowNodeResult(resultName,nextNodeCtlId,isDefault){\r\n      var than=this;\r\n      if(than.step.getNodeResult(resultName) != null && than.step.getNodeResult(resultName).nextNodeId > 0)\r\n      {\r\n        than.formItem[resultName]=parseInt(than.step.getNodeResult(resultName).nextNodeId);\r\n        than.nextNodeIdStr=parseInt(than.step.getNodeResult(resultName).nextNodeId);\r\n        //$(nextNodeCtlId).val(than.step.getNodeResult(resultName).nextNodeId+\"\");\r\n      }\r\n      // isDefault = isDefault != undefined && isDefault != null ? isDefault:0;\r\n      // $(nextNodeCtlId).attr(\"isDefault\",isDefault);\r\n    },\r\n    //保存NodeResult控件所选下一节点\r\n    SaveNodeResult(resultName,nextNodeCtlId,describe,isDefault){\r\n\r\n      var than=this;\r\n      if(than.step.getNodeResult(resultName) != null){\r\n        if(than.formItem.nextNodeId != null && than.formItem.nextNodeId != \"\")\r\n        {\r\n          var nodeResult = than.step.getNodeResult(resultName).restore({\r\n            nextNodeId:isNaN(than.formItem[resultName]) ? 0 : parseInt(than.formItem[resultName]) || 0,\r\n            isDefault:isDefault || 0\r\n          });\r\n          if(nodeResult != null) nodeResult.drawNodeResult();\r\n        }\r\n        else\r\n        {\r\n          than.step.removeNodeResult(resultName);\r\n        }\r\n      }\r\n      else{\r\n        if(than.formItem[resultName] != null && than.formItem[resultName] != \"\")\r\n        {\r\n          var nodeResult = than.step.createNodeResult({\r\n            name:resultName,\r\n            nextNodeId:isNaN(than.formItem[resultName]) ? 0 : parseInt(than.formItem[resultName]) || 0,\r\n            isDefault:isDefault || 0,\r\n            describe:describe\r\n          });\r\n          if(nodeResult != null) nodeResult.drawNodeResult();\r\n        }\r\n      }\r\n    },\r\n    btnVoice(){\r\n      var voice=this.$refs.getSelectData.selectRadioItem;\r\n      var voicePublic=this.$refs.getSelectData.$refs.publicVoiceManage.selectRadioItem;\r\n      var obj=voice;\r\n      if(voicePublic.id==undefined){\r\n        obj=voice;\r\n        obj.tag=0;\r\n      }else {\r\n        obj=voicePublic;\r\n        obj.tag=1;\r\n      }\r\n      this.formItem.voiceFile.id=obj.id;\r\n      this.formItem.voiceFile.name=\"voice\";\r\n      this.formItem.voiceFile.type=1;\r\n      this.formItem.voiceFile.content=obj.content;\r\n      this.formItem.voiceFile.describe=obj.audio;\r\n      this.formItem.voiceFile.tag=obj.tag;\r\n      this.voiceFileVisible=false;\r\n\r\n    }\r\n  },\r\n  mounted() {\r\n    this.init()\r\n  },\r\n  watch:{\r\n    'formItem.postParamsList':{\r\n      handler(v){\r\n        var str='';\r\n        v.forEach(function (v,i,arr){\r\n          str+=\"{\"+v+\"}\"\r\n        });\r\n        this.formItem.postParams=str;\r\n      },\r\n      deep:true\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.ui-combination-form{\r\n  .ui-voiceList{\r\n    .ui-voiceList-item{\r\n      text-align: left;\r\n      .ivu-form-item-label{\r\n        border: 1px solid red !important;\r\n      }\r\n    }\r\n  }\r\n\r\n}\r\n</style>"]}]}