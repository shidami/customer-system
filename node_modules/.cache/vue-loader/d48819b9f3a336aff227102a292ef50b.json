CA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"summary.vue","sourceRoot":"src/views/sys/customer-class","sourcesContent":["<style lang=\"less\">\r\n@import \"../../../styles/table-common.less\";\r\n</style>\r\n<template>\r\n  <div class=\"search\">\r\n    <Card>\r\n      <Row>\r\n        <Form ref=\"searchForm\" :model=\"searchForm\" inline :label-width=\"70\">\r\n          <Row>\r\n          <FormItem label=\"小结分类\" prop=\"summaryType\"> \r\n              <Input  type=\"text\"  v-model=\"searchForm.summaryType\" clearable style=\"width: 130px\" />\r\n          </FormItem>\r\n          <FormItem label=\"沟通内容\" prop=\"content\"> \r\n              <Input  type=\"text\"  v-model=\"searchForm.content\" clearable style=\"width: 130px\" />\r\n          </FormItem>\r\n          <Form-item style=\"margin-left:-35px;\" class=\"br\">\r\n            <Button @click.native=\"handleSearch\" type=\"primary\" icon=\"ios-search\">搜索</Button>\r\n            <Button @click=\"handleReset\">重置</Button>\r\n            <Button @click=\"addSummary\" type=\"primary\" icon=\"md-add\">添加服务小结</Button>\r\n          </Form-item>\r\n          </Row>\r\n        </Form>\r\n      </Row>\r\n      <Row v-show=\"openTip && selectCount > 0\">\r\n        <Alert show-icon>\r\n          已选择\r\n          <span class=\"select-count\">{{selectCount}}</span> 项\r\n          <a class=\"select-clear\" @click=\"clearSelectAll\">清空</a>\r\n        </Alert>\r\n      </Row>\r\n      <Row>\r\n        <Table\r\n          :loading=\"loading\"\r\n          :columns=\"columns\"\r\n          :data=\"data\"\r\n          sortable=\"custom\"\r\n          @on-sort-change=\"changeSort\"\r\n          @on-selection-change=\"showSelect\"\r\n          ref=\"table\"\r\n        ></Table>\r\n      </Row>\r\n      <Row type=\"flex\" justify=\"end\" class=\"page\">\r\n        <Page\r\n          :current=\"searchForm.pageNumber\"\r\n          :total=\"total\"\r\n          :page-size=\"searchForm.pageSize\"\r\n          @on-change=\"changePage\"\r\n          @on-page-size-change=\"changePageSize\"\r\n          :page-size-opts=\"[10,20,50]\"\r\n          size=\"small\"\r\n          show-total\r\n          show-elevator\r\n          show-sizer\r\n        ></Page>\r\n      </Row>\r\n    </Card>\r\n\r\n\r\n    <!-- 新增编辑 -->\r\n    <Modal :title=\"modalTitle\" v-model=\"ModalVisible\" :mask-closable=\"false\" :width=\"500\">\r\n      <Form ref=\"summaryForm\" :model=\"summaryForm\" :label-width=\"80\">\r\n       \r\n        <FormItem label=\"小结分类\" prop=\"summaryType\">\r\n          <Input v-model=\"summaryForm.summaryType\"  />\r\n        </FormItem>\r\n        <FormItem label=\"沟通内容\" prop=\"content\">\r\n          <Input type=\"textarea\" v-model=\"summaryForm.content\" :rows=\"4\" />\r\n        </FormItem>\r\n         \r\n      </Form>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"cancelSummary\">取消</Button>\r\n        <Button type=\"primary\" :loading=\"submitLoading\" @click=\"submitSummary\">提交</Button>\r\n      </div>\r\n    </Modal>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  getAllSummaryList, \r\n  addSummary, \r\n  editSummary, \r\n  deleteSummary\r\n} from \"@/api/index\";\r\nexport default {\r\n  name: \"summary-manage\",\r\n  data() {\r\n    return {\r\n      height: 510,\r\n      openTip: true,\r\n      loading: false,\r\n      selectCount: 0,\r\n      selectList: [],\r\n      modalTitle:\"\", \r\n      ModalVisible:false,\r\n      modalType: 0,\r\n      summaryForm:{    \r\n        summaryType:\"\",\r\n        content:\"\"\r\n      },\r\n      searchForm: {\r\n        summaryType:\"\",\r\n        content:\"\",\r\n        pageNumber: 1,\r\n        pageSize: 10,\r\n        sort: \"Id\",\r\n        order: \"desc\"\r\n      },\r\n      columns: [\r\n        {\r\n            title: \"序号\", //序号\r\n            type: \"index\",\r\n            width: 60,\r\n            align: \"center\",\r\n        },\r\n        {  title: \"小结类型\", key: \"summaryType\", minWidth: 125},\r\n        {  title: \"沟通内容\", key: \"content\", minWidth: 125},\r\n        {\r\n            title: this.$t(\"operation\"), //操作\r\n            key: \"active\",\r\n            width: 100,\r\n            'checkeds':true, //自定义属性，默认选中，必须展示的数据\r\n            render: (h, params) => {\r\n                return h(\"div\", [\r\n                    //编辑\r\n                    h('Tooltip',{props: {\r\n                            placement: 'top',\r\n                            transfer: true\r\n                        }},[h(\r\n                        \"Icon\",\r\n                        {\r\n                            props: {\r\n                                type: \"ios-create-outline\",\r\n                                size: \"\",\r\n                            },\r\n                            style: { marginRight: \"5px\",fontSize:'18px',cursor: 'pointer' },\r\n                            on: {\r\n                                click: () => {\r\n                                    this.edit(params.row);\r\n                                },\r\n                            },\r\n                        },\r\n                        this.$t(\"play\")\r\n                    ),h('span', {\r\n                        slot: 'content',\r\n                        style: {\r\n                            whiteSpace: 'normal'\r\n                        }\r\n                    }, '编辑')]),\r\n\r\n\r\n                    //删除\r\n                    h('Tooltip',{props: {\r\n                            placement: 'top',\r\n                            transfer: true\r\n                        }},[h(\r\n                        \"Icon\",\r\n                        {\r\n                            props: {\r\n                                type: \"ios-trash-outline\",\r\n                                size: \"\",\r\n                            },\r\n                            style: { marginRight: \"5px\",fontSize:'18px',cursor: 'pointer' },\r\n                            on: {\r\n                                click: () => {\r\n                                    this.remove(params.row);\r\n                                },\r\n                            },\r\n                        },\r\n                        this.$t(\"play\")\r\n                    ),h('span', {\r\n                        slot: 'content',\r\n                        style: {\r\n                            whiteSpace: 'normal'\r\n                        }\r\n                    }, '删除')])\r\n                    ]);\r\n                }\r\n            }\r\n      ],\r\n      data: [],\r\n      total: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.getSummaryList();\r\n    },\r\n    changeSelect(e) {\r\n      this.selectList = e;\r\n      this.selectCount = e.length;\r\n      if(this.selectCount > 0){\r\n         this.openTip = true;\r\n      }else{\r\n         this.openTip = false;\r\n      }\r\n    },\r\n    handleSearch() {\r\n      this.selectList = \"\";\r\n      this.selectCount = 0;\r\n      this.searchForm.pagember = 1;\r\n      this.searchForm.pageSize = 10;\r\n      this.getSummaryList();\r\n    },\r\n    handleReset() {\r\n      this.$refs.searchForm.resetFields();\r\n      this.searchForm.pageNumber = 1;\r\n      this.searchForm.pageSize = 10;\r\n    },\r\n    changePage(v) {\r\n      this.searchForm.pageNumber = v;\r\n      this.getSummaryList();\r\n      this.clearSelectAll();\r\n    },\r\n    changePageSize(v) {\r\n      this.searchForm.pageSize = v;\r\n      this.getSummaryList();\r\n    },\r\n    clearSelectAll() {\r\n      this.$refs.table.selectAll(false);\r\n    },\r\n    showSelect(e) {\r\n      this.selectList = e;\r\n      this.selectCount = e.length;\r\n    },\r\n    changeSort(e) {\r\n      this.searchForm.sort = e.key;\r\n      this.searchForm.order = e.order;\r\n      if (e.order == \"normal\") {\r\n        this.searchForm.order = \"\";\r\n      }\r\n      this.getSummaryList();\r\n    },\r\n    getSummaryList() {\r\n        this.loading = true;\r\n        var formdata={};\r\n        for(var i in this.searchForm){\r\n          if(this.searchForm[i] != null && this.searchForm[i] !=\"\"){\r\n            formdata[i] = this.searchForm[i]; \r\n          }\r\n        }\r\n        getAllSummaryList(formdata).then(res => {\r\n          this.loading = false;\r\n          if (res.success) {\r\n            this.data = res.result.list;\r\n            this.total = res.result.total;\r\n          }\r\n        });\r\n    },\r\n    addSummary(){\r\n          this.modalTitle=\"新增服务小结\";\r\n          this.ModalVisible = true;\r\n          this.modalType = 0;\r\n          this.$refs.summaryForm.resetFields();\r\n    },\r\n    edit(v) {\r\n      var formdata={};\r\n      for(let attr in v){\r\n        if(v[attr] != null && v[attr] !=\"\"){\r\n          formdata[attr] = v[attr]+\"\"; \r\n        }\r\n      }\r\n      this.modalType = 1;\r\n      this.ModalVisible = true;\r\n      this.modalTitle = \"编辑服务小结\";\r\n      this.summaryForm = formdata;\r\n    },\r\n    submitSummary() {\r\n          var formdata={};\r\n          if(!this.summaryForm.summaryType) {\r\n            this.$Message.warning(\"小结分类不能为空!\");\r\n            return;\r\n          }\r\n          for(var i in this.summaryForm){\r\n            if(this.summaryForm[i] != null && this.summaryForm[i] !=\"\"){\r\n                 formdata[i] = this.summaryForm[i];  \r\n            }\r\n          }\r\n          if (this.modalType == 0) {\r\n            // 添加\r\n            this.submitLoading = true;\r\n            addSummary(formdata).then(res => {\r\n              this.submitLoading = false;\r\n              if (res.success) {\r\n                this.$Message.success(\"操作成功\");\r\n                this.getSummaryList();\r\n                this.ModalVisible = false;\r\n              }\r\n            });\r\n          } else {\r\n            this.submitLoading = true;\r\n            editSummary(formdata).then(res => {\r\n              this.submitLoading = false;\r\n              if (res.success) {\r\n                this.$Message.success(\"操作成功\");\r\n                this.getSummaryList();\r\n                this.ModalVisible = false;\r\n              }\r\n            });\r\n        }\r\n    },\r\n    remove(v) {\r\n      this.$Modal.confirm({\r\n        title: \"确认删除\",\r\n        // 记得确认修改此处\r\n        content: \"您确认要删除 \" + v.summaryType + \" ?\",\r\n        loading: true,\r\n        onOk: () => {\r\n          // 删除\r\n          deleteSummary({ ids: v.id }).then((res) => {\r\n            this.$Modal.remove();\r\n            if (res.success) {\r\n              this.$Message.success(\"操作成功\");\r\n              this.getSummaryList();\r\n              this.selectCount = 0;\r\n            }\r\n          });\r\n        },\r\n      });\r\n    },\r\n    cancelSummary() {\r\n      this.ModalVisible = false;\r\n    }\r\n  },\r\n  mounted() {\r\n    // 计算高度\r\n    this.height = Number(document.documentElement.clientHeight - 230);\r\n    this.init();\r\n  }\r\n};\r\n</script>"]}]}