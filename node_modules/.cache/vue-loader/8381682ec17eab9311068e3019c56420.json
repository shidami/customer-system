{"remainingRequest":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\src\\views\\my-components\\IVR\\SetNode_Database.vue?vue&type=style&index=0&id=6b40621c&scoped=true&lang=less&","dependencies":[{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\src\\views\\my-components\\IVR\\SetNode_Database.vue","mtime":1614579763281},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\less-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["SetNode_Database.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyjBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA","file":"SetNode_Database.vue","sourceRoot":"src/views/my-components/IVR","sourcesContent":["<template>\r\n  <Row style=\"padding-top: 20px;background: #fff;\">\r\n    <Col span=\"22\" offset=\"1\">\r\n      <!--      {{formItem}}-->\r\n      <!--      {{nextNodeIdStr}}-->\r\n      <Form class=\"ui-combination-form\" ref=\"formItem\" :model=\"formItem\" :rules=\"formItemValidate\" :label-width=\"120\">\r\n        <FormItem label=\"节点号\" prop=\"id\">\r\n          <Input v-model=\"formItem.id\" placeholder=\"请输入...\" disabled></Input>\r\n        </FormItem>\r\n        <FormItem label=\"节点名称\" prop=\"name\">\r\n          <Input v-model=\"formItem.name\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"数据库类型\" prop=\"dbType\">\r\n          <Select v-model=\"formItem.dbType\">\r\n            <Option value=\"1\">Oracle</Option>\r\n            <Option value=\"2\">SQLServer</Option>\r\n            <Option value=\"3\">MySql</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"dbURI\" prop=\"dbURI\" class=\"ui-voiceList-item\">\r\n          <Input v-model=\"formItem.dbURI\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"端口号\" prop=\"dbPort\" class=\"ui-voiceList-item\">\r\n          <Input v-model=\"formItem.dbPort\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"数据库名\" prop=\"dbName\" class=\"ui-voiceList-item\">\r\n          <Input v-model=\"formItem.dbName\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"用户名\" prop=\"dbUserName\" class=\"ui-voiceList-item\">\r\n          <Input v-model=\"formItem.dbUserName\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem label=\"密码\" prop=\"dbPassword\" class=\"ui-voiceList-item\">\r\n          <Input type=\"password\" v-model=\"formItem.dbPassword\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n<!--        <FormItem label=\"SQL语句\" prop=\"dbSQL\" class=\"ui-voiceList-item\">-->\r\n<!--          <Input type=\"password\" v-model=\"formItem.dbSQL\" placeholder=\"请输入...\"></Input>-->\r\n<!--        </FormItem>-->\r\n        <FormItem label=\"SQL语句\" prop=\"dbSQL\" class=\"ui-voiceList-item\">\r\n          <Row :gutter=\"16\" style=\"margin-bottom: 5px\">\r\n            <Col span=\"15\" prop=\"dataSource\" style=\"padding-left: 0;\">\r\n              <Input type=\"textarea\" v-model=\"formItem.dbSQL\"  placeholder=\"请选择\" @on-change=\"ChangePostParams(formItem.dbSQL)\"></Input>\r\n            </Col>\r\n            <Col span=\"8\" style=\"text-align: left;padding-left: 0 !important;\">\r\n              <Button @click=\"selProcessVariablesVisible('list')\" style=\"margin-left: 10px\">选择</Button>\r\n            </Col>\r\n          </Row>\r\n        </FormItem>\r\n        <FormItem label=\"返回值变量名\" prop=\"returnVariable\" class=\"ui-voiceList-item\">\r\n          <Row :gutter=\"16\" style=\"margin-bottom: 5px\">\r\n            <Col span=\"15\" prop=\"returnVariable\" style=\"padding-left: 0;\">\r\n              <Input type=\"textarea\" v-model=\"formItem.returnVariable\" readonly placeholder=\"请选择\" @on-change=\"ChangereturnVariable(formItem.returnVariable)\"></Input>\r\n            </Col>\r\n            <Col span=\"8\" style=\"text-align: left;padding-left: 0 !important;\">\r\n              <Button @click=\"variableMappingVisible=true\" style=\"margin-left: 10px\">选择</Button>\r\n            </Col>\r\n          </Row>\r\n        </FormItem>\r\n        <FormItem label=\"跳转节点\" prop=\"nextNodeId\">\r\n          <Select v-model=\"formItem.nextNodeId\">\r\n            <Option :value=\"item.value\" v-for=\"(item,i) in nextNodeIdList\" :key=\"i\">{{item.text}}</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"说明\" prop=\"describe\">\r\n          <Input type=\"textarea\" v-model=\"formItem.describe\" placeholder=\"请输入...\"></Input>\r\n        </FormItem>\r\n        <FormItem>\r\n          <Button @click=\"btnOk\" type=\"primary\">确定</Button>\r\n          <Button @click=\"btnCancel\" style=\"margin-left: 8px\">取消</Button>\r\n        </FormItem>\r\n      </Form>\r\n    </Col>\r\n    <Modal\r\n        title=\"选择流程变量\"\r\n        v-model=\"processVariablesVisible\"\r\n        :closable=\"false\"\r\n        :mask-closable=\"false\"\r\n        :width=\"800\"\r\n    >\r\n      <processVariables ref=\"getProcessVariablesVisible\"></processVariables>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"processVariablesVisible = false\">{{\r\n            $t(\"cancel\")\r\n          }}</Button>\r\n        <Button\r\n            type=\"primary\"\r\n            @click=\"btnProcessVariables\"\r\n        >{{ $t(\"save\") }}</Button\r\n        >\r\n      </div>\r\n    </Modal><!--选择流程变量-结束--->\r\n\r\n\r\n    <Modal\r\n        title=\"变量映射\"\r\n        v-model=\"variableMappingVisible\"\r\n        :closable=\"false\"\r\n        :mask-closable=\"false\"\r\n        :width=\"800\"\r\n    >\r\n      <div style=\"text-align: right;margin-bottom: 15px;\">\r\n        <Button @click=\"addTableList\" type=\"primary\" icon=\"md-add\">{{$t(\"add\")}}</Button><!-- 增加 -->\r\n      </div>\r\n      <div>\r\n        <table class=\"ivu-table\" cellspacing=\"0\" cellpadding=\"0\" border=\"0\" width=\"100%\" style=\"text-align: center;vertical-align: middle\">\r\n          <thead>\r\n            <tr>\r\n              <th>编号</th>\r\n              <th>变量名</th>\r\n              <th>字段名</th>\r\n              <th>操作</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"(item,i) in formItem.returnVariableList\" :key=\"i\">\r\n              <td>{{i+1}}</td>\r\n              <td style=\"width: 25%\">\r\n                <Form>\r\n                  <FormItem prop=\"item.name\" style=\"margin-bottom: 0;\">\r\n                    <Input @click.native=\"selProcessVariablesVisible('listItem',i)\" v-model=\"item.name\" placeholder=\"请输入...\"></Input>\r\n                  </FormItem>\r\n                </Form>\r\n              </td>\r\n              <td style=\"width: 25%;padding-left: 1%\">\r\n                <Form>\r\n                  <FormItem prop=\"item.value\" style=\"margin-bottom: 0;\">\r\n                    <Input v-model=\"item.value\" placeholder=\"请输入...\"></Input>\r\n                  </FormItem>\r\n                </Form>\r\n              </td>\r\n              <td>\r\n                <Icon @click=\"rowDel(i)\" type=\"ios-trash-outline\" style=\"font-size: 20px\"/>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"variableMappingVisible = false\">{{\r\n            $t(\"cancel\")\r\n          }}</Button>\r\n        <Button\r\n            type=\"primary\"\r\n            @click=\"btnvariableMapping\"\r\n        >{{ $t(\"save\") }}</Button\r\n        >\r\n      </div>\r\n    </Modal><!--变量映射-结束--->\r\n  </Row>\r\n</template>\r\n\r\n<script>\r\nimport {getBlackListGroups} from \"@/api/index\";\r\nimport processVariables from \"@/views/my-components/IVR/processVariables\";\r\nvar frame = parent;\r\nexport default {\r\n  name: \"SetNode_Database\",\r\n  components:{processVariables},\r\n  data(){\r\n    return{\r\n      layParent:true,\r\n      variableMappingVisible:false,\r\n      processVariablesVisible:false,\r\n      selProcessVariablesType:'str',\r\n      selProcessVariablesIndex:0,\r\n      openerid:WFCore.GetQuery(\"openerid\"),\r\n      stepid:WFCore.GetQuery(\"id\"),\r\n      flowdata:top.wMyflowData,\r\n      nextNodeIdList:[{value:0,text:'无'}],\r\n      selIndex:0,\r\n      step:'',\r\n      formItem:{\r\n        id:'',//节点号\r\n        name:'',//节点名称\r\n        dbType:'1',//数据库类型\r\n        httpURL:'',//URL\r\n        dbPort:'',//端口号\r\n        dbName:'',//数据库名\r\n        dbUserName:'',//用户名\r\n        dbPassword:'',//密码\r\n        dbSQL:'',//SQL语句\r\n        //dbSQL:'',//参数\r\n        dbSQLList:[],//\r\n        returnVariable:'',//返回值变量名\r\n        returnVariableList:[{name:'',value:''}],//\r\n\r\n        strVariable:'',//变量名\r\n        dataSource:'',//源数据\r\n        dataSourceList:[],//源数据\r\n        nextNodeId:0,//跳转节点\r\n        sort:'',//排序序号\r\n        describe:'',//说明\r\n      },\r\n      inBlacklistNextNodeIdStr:0,//黑名单跳转节点-绑定\r\n      notInBlacklistNextNodeIdStr:0,//非黑名单跳转节点-绑定\r\n      formItemValidate:{\r\n        name: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n        // dbURI: [{\r\n        //   required: true,\r\n        //   message: this.$t(\"pleaseEnter\"),\r\n        //   trigger: \"blur\",\r\n        // }],\r\n        dbPort: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n        dbName: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n        dbUserName: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n        dbPassword: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n        dbSQL: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }],\r\n        returnVariable: [{\r\n          required: true,\r\n          message: this.$t(\"pleaseEnter\"),\r\n          trigger: \"blur\",\r\n        }]\r\n      },\r\n      columns1: [\r\n        {\r\n          title: \"编号\", //序号\r\n          type: \"index\",\r\n          width: 60,\r\n          align: \"center\",\r\n        },\r\n        {\r\n          title: '变量名',\r\n          key: 'name',\r\n          editable: true,\r\n          render: (h, params) => {\r\n            return  this.setDataColumn(params,h,'name');\r\n          }\r\n        },\r\n        {\r\n          title: '字段名',\r\n          key: 'value',\r\n          editable: true,\r\n          render: (h, params) => {\r\n            return  this.setDataColumn(params,h,'value');\r\n          }\r\n        },\r\n        {\r\n          title: '操作',\r\n          key: 'type',\r\n          editable: true,\r\n          render: (h, params) => {\r\n            return h(\"div\", [\r\n              //删除\r\n              h('Tooltip',{props: {\r\n                  placement: 'top',\r\n                  transfer: true\r\n                }},[h(\r\n                  \"Icon\",\r\n                  {\r\n                    props: {\r\n                      type: \"ios-trash-outline\",\r\n                      size: \"\",\r\n                    },\r\n                    style: { marginRight: \"5px\",fontSize:'18px',cursor: 'pointer'},\r\n                    on: {\r\n                      click: () => {\r\n                        this.rowDel(params.index,params.row);\r\n                      },\r\n                    },\r\n                  },\r\n                  this.$t(\"play\")\r\n              ),h('span', {\r\n                slot: 'content',\r\n                style: {\r\n                  whiteSpace: 'normal'\r\n                }\r\n              }, '删除')])\r\n\r\n            ]);\r\n          },\r\n        }\r\n      ],\r\n      data1:[{\r\n        name:'',\r\n        value:'',\r\n      }]\r\n    }\r\n  },\r\n  methods:{\r\n    init(){\r\n      this.initData();\r\n    },\r\n    initData(){\r\n      var than=this;\r\n      if (frame == null) return;\r\n      than.flowdata=top.wMyflowData;\r\n      than.step = than.flowdata.getNode(than.stepid);\r\n      if(than.flowdata.nodes.size()>0){\r\n        than.flowdata.nodes.each(function (nodeId,node,index){\r\n          if(than.step.checkNextNodeIsAllow(\"nextNodeId\",node.id)){\r\n            than.nextNodeIdList.push({value:node.id,text:[node.id]+'-' + node.getText()})\r\n          }\r\n        })\r\n      }\r\n\r\n      this.initStep(than.step);\r\n    },\r\n    initStep(step) {\r\n      var than=this;\r\n      if(step==null||step==undefined){\r\n        than.formItem.name='新流程';\r\n        return;\r\n      }\r\n      than.formItem.id=step.id;\r\n      than.formItem.name=step.getText();\r\n      than.formItem.dbType=step.dbType;\r\n      than.formItem.dbURI=step.dbURI;\r\n      than.formItem.dbPort=step.dbPort;\r\n      than.formItem.dbName=step.dbName;\r\n      than.formItem.dbUserName=step.dbUserName;\r\n      than.formItem.dbPassword=step.dbPassword;\r\n      than.formItem.dbSQL=step.dbSQL;\r\n      than.formItem.returnVariable=step.returnVariable;\r\n      than.formItem.sort=step.sort;\r\n      than.formItem.describe=step.describe;\r\n\r\n      //跳转节点\r\n      than.ShowNodeResult(\"nextNodeId\",\"#nextNodeId\",1);\r\n      //跳转节点\r\n      // than.ShowNodeResult(\"nextNodeId\",\"#base_NextNodeId\",0);\r\n      window.setTimeout(function (){\r\n        than.formItem.nextNodeId=than.nextNodeIdStr;\r\n      },100)\r\n    },\r\n    btnOk(){\r\n      var than=this;\r\n      var isFormValid = new RoadUI.Validate().validateForm(\"form1\");\r\n      if(this.formItem.replayCount > 10||this.formItem.replayCount <1)\r\n      {\r\n        WFCore.DialogUtils.tipDialog(\"重播次数在1-10之间\",2,7);\r\n        return false;\r\n      }\r\n      this.$refs[\"formItem\"].validate((valid) => {\r\n        if (valid) {\r\n          than.confirm1();\r\n        }\r\n      })\r\n    },\r\n    btnCancel(){\r\n      new RoadUI.Window().close();\r\n    },\r\n    confirm1() {\r\n      var than=this;\r\n\r\n      var ostep={\r\n        text:{\r\n          text:than.formItem.name\r\n        },\r\n        sort:than.formItem.sort,\r\n        describe:than.formItem.describe,\r\n        dbType:than.formItem.dbType,\r\n        dbURI:than.formItem.dbURI,\r\n        dbPort:than.formItem.dbPort,\r\n        dbName:than.formItem.dbName,\r\n        dbUserName:than.formItem.dbUserName,\r\n        dbPassword:than.formItem.dbPassword,\r\n        dbSQL:than.formItem.dbSQL,\r\n        returnVariable:than.formItem.returnVariable\r\n      }\r\n\r\n      //非黑名单跳转节点\r\n      than.SaveNodeResult(\"nextNodeId\",\"#nextNodeId\",\"跳转节点\",1);\r\n\r\n      var playVoices = new parent.WFCore.List();\r\n      var varIndex = 1;\r\n\r\n      window.setTimeout(function (){\r\n        than.step.restore(ostep);\r\n        new RoadUI.Window().close();\r\n      },100)\r\n    },\r\n    //给NodeResult控件赋值\r\n    ShowNodeResult(resultName,nextNodeCtlId,isDefault){\r\n      var than=this;\r\n      if(than.step.getNodeResult(resultName) != null && than.step.getNodeResult(resultName).nextNodeId > 0)\r\n      {\r\n        than.formItem[resultName]=parseInt(than.step.getNodeResult(resultName).nextNodeId);\r\n        than.nextNodeIdStr=parseInt(than.step.getNodeResult(resultName).nextNodeId);\r\n        //$(nextNodeCtlId).val(than.step.getNodeResult(resultName).nextNodeId+\"\");\r\n      }\r\n      // isDefault = isDefault != undefined && isDefault != null ? isDefault:0;\r\n      // $(nextNodeCtlId).attr(\"isDefault\",isDefault);\r\n    },\r\n    //保存NodeResult控件所选下一节点\r\n    SaveNodeResult(resultName,nextNodeCtlId,describe,isDefault){\r\n\r\n      var than=this;\r\n      if(than.step.getNodeResult(resultName) != null){\r\n        if(than.formItem.nextNodeId != null && than.formItem.nextNodeId != \"\")\r\n        {\r\n          var nodeResult = than.step.getNodeResult(resultName).restore({\r\n            nextNodeId:isNaN(than.formItem[resultName]) ? 0 : parseInt(than.formItem[resultName]) || 0,\r\n            isDefault:isDefault || 0\r\n          });\r\n          if(nodeResult != null) nodeResult.drawNodeResult();\r\n        }\r\n        else\r\n        {\r\n          than.step.removeNodeResult(resultName);\r\n        }\r\n      }\r\n      else{\r\n        if(than.formItem[resultName] != null && than.formItem[resultName] != \"\")\r\n        {\r\n          var nodeResult = than.step.createNodeResult({\r\n            name:resultName,\r\n            nextNodeId:isNaN(than.formItem[resultName]) ? 0 : parseInt(than.formItem[resultName]) || 0,\r\n            isDefault:isDefault || 0,\r\n            describe:describe\r\n          });\r\n          if(nodeResult != null) nodeResult.drawNodeResult();\r\n        }\r\n      }\r\n    },\r\n    //\r\n    selVoiceListItem(i){\r\n      this.selIndex=i;\r\n    },\r\n    selProcessVariablesVisible(v,i){\r\n      this.processVariablesVisible=true;\r\n      this.selProcessVariablesType=v;\r\n      this.selProcessVariablesIndex=i==undefined?0:i;\r\n    },\r\n    btnProcessVariables(){\r\n      var than=this;\r\n      if(this.selProcessVariablesType=='list'){\r\n        this.formItem.dbSQLList.push(this.$refs.getProcessVariablesVisible.selectRadioItem.name);\r\n\r\n      }else {\r\n        than.formItem.returnVariableList[than.selProcessVariablesIndex].name=this.$refs.getProcessVariablesVisible.selectRadioItem.name;\r\n      }\r\n\r\n      this.processVariablesVisible=false;\r\n    },\r\n    ChangePostParams(v){\r\n      var arr0=[];\r\n      var arr1=[];\r\n      if(v==\"\"){\r\n        debugger\r\n        this.formItem.dataSourceList=[];\r\n        return  false;\r\n      }\r\n          arr0=v.split(\"{\");//字符分割\r\n      arr0.forEach(function (v,i,arr){\r\n        if(v.toString()!=\"{}\"&&v.toString()!=''){\r\n          var str=v;\r\n          if(str.substring(0,1)==\"{\"){\r\n            str=str.replace(\"{\",'');\r\n          }\r\n          if(str.substring(str.length-1)==\"}\"){\r\n            str=str.replace(\"}\",'');\r\n          }\r\n          arr1.push(str)\r\n        }\r\n      })\r\n      this.formItem.dataSourceList=arr1;\r\n    },\r\n    ChangereturnVariable(){\r\n\r\n    },\r\n    btnvariableMapping(){\r\n      var than=this;\r\n      var str=[];\r\n      than.formItem.returnVariableList.forEach(function (v,i,arr){\r\n        str.push('{'+v.name+'}='+v.value);\r\n      })\r\n      than.formItem.returnVariable=str.toString();\r\n      than.variableMappingVisible=false;\r\n      console.log(str);\r\n    },\r\n    setDataColumn(params,h,name){\r\n      var str=null;\r\n      var than=this;\r\n      if(params.column.select!=undefined){\r\n        return h('Select', {\r\n          props: {\r\n            value: params.row[name]\r\n          },\r\n          on: {\r\n            \"on-change\": event => {\r\n\r\n              params.row[name] = event;\r\n              than.data1[params.index][name] = event;\r\n            }\r\n          }\r\n        },params.column.option.map((item)=>{\r\n          return h('Option',{\r\n            props:{\r\n              value:item.key,\r\n              label:item.val\r\n            }\r\n          })\r\n        }));\r\n      }else {\r\n        return str= h('Input', {\r\n          props: {\r\n            type: \"text\",\r\n            value: params.row[name]\r\n          },\r\n\r\n          on: {\r\n            \"on-change\": event => {\r\n              debugger\r\n              params.row[name] = event.target.value;\r\n              than.data1[params.index][name] = event.target.value;\r\n            },\r\n            \"click.native\": event => {\r\n              than.processVariablesVisible=true\r\n            }\r\n\r\n          }\r\n        });\r\n      }\r\n      return str;\r\n    },\r\n    addTableList(){\r\n      var than=this;\r\n      than.formItem.returnVariableList.push({\r\n        name: \"\",\r\n        value:\"\",\r\n      })\r\n    },\r\n    rowDel(i,v){\r\n      console.log(v)\r\n      this.formItem.returnVariableList.splice(i,1);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.init()\r\n  },\r\n  watch:{\r\n    'formItem.dbSQLList':{\r\n      handler(v){\r\n        var str='';\r\n        v.forEach(function (v,i,arr){\r\n          str+=\"{\"+v+\"}\"\r\n        });\r\n        this.formItem.dbSQL=str;\r\n      },\r\n      deep:true\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n.ui-combination-form{\r\n  .ui-voiceList{\r\n    .ui-voiceList-item{\r\n      text-align: left;\r\n      .ivu-form-item-label{\r\n        border: 1px solid red !important;\r\n      }\r\n    }\r\n  }\r\n\r\n}\r\n</style>"]}]}