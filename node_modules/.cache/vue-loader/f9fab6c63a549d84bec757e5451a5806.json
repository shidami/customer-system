nput  type=\"text\"  v-model=\"searchForm.name\" clearable style=\"width: 130px\" />\r\n          </FormItem>\r\n          <Form-item style=\"margin-left:-35px;\" class=\"br\">\r\n            <Button @click.native=\"handleSearch\" type=\"primary\" icon=\"ios-search\">搜索</Button>\r\n            <Button @click=\"handleReset\">重置</Button>\r\n          </Form-item>\r\n          </Row>\r\n        </Form>\r\n      </Row>\r\n      <Row>\r\n        <div class=\"operation ui-operation\">\r\n          <div class=\"ui-operation-left\" v-show=\"openTip && selectCount > 0\">\r\n            <Row class=\"ui-alert-warp\">\r\n              <Alert>\r\n                已选择\r\n                <span class=\"select-count\">{{selectCount}}</span> 项\r\n                <a class=\"select-clear\" @click=\"clearSelectAll\">清空</a>\r\n              </Alert>\r\n            </Row>\r\n          </div>\r\n          <div class=\"ui-operation-right\" v-show=\"openTip && selectCount <= 0\">\r\n            <Button @click=\"addIvrWorkFlow\" type=\"primary\" icon=\"md-add\">添加IVR流程</Button>\r\n            <Button type=\"dashed\" @click=\"openSearch=!openSearch\">{{openSearch ? \"关闭搜索\" : \"开启搜索\"}}</Button>\r\n          </div>\r\n        </div>\r\n\r\n\r\n      </Row>\r\n      <Row>\r\n        <Table\r\n          :loading=\"loading\"\r\n          :columns=\"columns\"\r\n          :data=\"data\"\r\n          sortable=\"custom\"\r\n          @on-sort-change=\"changeSort\"\r\n          @on-selection-change=\"showSelect\"\r\n          ref=\"table\"\r\n        ></Table>\r\n      </Row>\r\n      <Row type=\"flex\" justify=\"end\" class=\"page\">\r\n        <Page\r\n          :current=\"searchForm.pageNumber\"\r\n          :total=\"total\"\r\n          :page-size=\"searchForm.pageSize\"\r\n          @on-change=\"changePage\"\r\n          @on-page-size-change=\"changePageSize\"\r\n          :page-size-opts=\"[10,20,50]\"\r\n          size=\"small\"\r\n          show-total\r\n          show-elevator\r\n          show-sizer\r\n        ></Page>\r\n      </Row>\r\n    </Card>\r\n\r\n    <IVRVoiceDefault :IVRVoiceDefaultModalVisible=\"IVRVoiceDefaultModalVisible\" :IVRFormData=\"IVRFormData\"></IVRVoiceDefault>\r\n\r\n    <!-- 新增编辑 -->\r\n    <Modal :title=\"modalTitle\" v-model=\"ModalVisible\" :mask-closable=\"false\" :width=\"500\">\r\n      <Form ref=\"ivrWorkFlowForm\" :model=\"ivrWorkFlowForm\" :rules=\"ivrWorkFlowFormValidate\" :label-width=\"80\">\r\n\r\n        <FormItem label=\"流程名称\" prop=\"name\">\r\n          <Input v-model=\"ivrWorkFlowForm.name\"  />\r\n        </FormItem>\r\n        <FormItem label=\"版本号\" prop=\"wfversion\">\r\n          <Input v-model=\"ivrWorkFlowForm.wfversion\"  />\r\n        </FormItem>\r\n        <FormItem label=\"备注\" prop=\"remark\">\r\n          <Input type=\"textarea\" v-model=\"ivrWorkFlowForm.remark\" :rows=\"4\" />\r\n        </FormItem>\r\n\r\n      </Form>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"cancelIvrWorkFlow\">取消</Button>\r\n        <Button type=\"primary\" :loading=\"submitLoading\" @click=\"submitIvrWorkFlow\">提交</Button>\r\n      </div>\r\n    </Modal>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport IVRVoiceDefault from \"@/views/my-components/IVR/default\";\r\nimport {\r\n  getAllIvrWorkFlowList,   \r\n  addIvrWorkFlow, \r\n  editIvrWorkFlow, \r\n  deleteIvrWorkFlow\r\n} from \"@/api/index\";\r\nexport default {\r\n  name: \"ivrworkflow-manage\",\r\n  components:{IVRVoiceDefault},\r\n  data() {\r\n    return {\r\n      IVRVoiceDefaultModalVisibleTitle:'新增',\r\n      IVRVoiceDefaultModalVisible:false,\r\n      openSearch:false,\r\n      height: 510,\r\n      openTip: true,\r\n      loading: false,\r\n      selectCount: 0,\r\n      selectList: [],\r\n      modalTitle:\"\", \r\n      ModalVisible:false,\r\n      modalType: 0,\r\n      IVRFormData:{},\r\n      ivrWorkFlowForm:{    \r\n        name:\"\",\r\n        wfversion:\"\", \r\n        remark:\"\"\r\n      },\r\n      ivrWorkFlowFormValidate:{\r\n        name: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"pleaseEnter\"),\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        wfversion: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"pleaseEnter\"),\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        remark: [\r\n          {\r\n            required: true,\r\n            message: this.$t(\"pleaseEnter\"),\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n      },\r\n      searchForm: {\r\n        name:\"\",\r\n        pageNumber: 1,\r\n        pageSize: 10,\r\n        sort: \"createTime\",\r\n        order: \"desc\"\r\n      },\r\n      columns: [\r\n        {\r\n          title: \"复选框\", //复选框\r\n          type: \"selection\",\r\n          width: 60,\r\n          align: \"center\",\r\n          'checkeds':true //自定义属性，默认选中，必须展示的数据\r\n        },\r\n        {\r\n            title: \"序号\", //序号\r\n            type: \"index\",\r\n            width: 60,\r\n            align: \"center\",\r\n        },\r\n        {  title: \"流程名称\", key: \"name\", minWidth: 125},\r\n        {  title: \"创建时间\", key: \"createTime\", minWidth: 125},\r\n        {  title: \"创建人\", key: \"createBy\", minWidth: 125},\r\n        {  title: \"发布时间\", key: \"inStallDate\", minWidth: 125},\r\n        {  title: \"发布人\", key: \"createBy\", minWidth: 125},\r\n        {  title: \"流程状态\", key: \"status\", minWidth: 125},\r\n        {  title: \"流程版本\", key: \"wfversion\", minWidth: 125},\r\n        {\r\n            title: this.$t(\"operation\"), //操作\r\n            key: \"active\",\r\n            width: 150,\r\n            'checkeds':true, //自定义属性，默认选中，必须展示的数据\r\n            render: (h, params) => {\r\n                return h(\"div\", [\r\n                    //编辑\r\n                    h('Tooltip',{props: {\r\n                            placement: 'top',\r\n                            transfer: true\r\n                        }},[h(\r\n                        \"Icon\",\r\n                        {\r\n                            props: {\r\n                                type: \"ios-create-outline\",\r\n                                size: \"\",\r\n                            },\r\n                            style: { marginRight: \"5px\",fontSize:'18px',cursor: 'pointer' },\r\n                            on: {\r\n                                click: () => {\r\n                                    this.edit(params.row);\r\n                                },\r\n                            },\r\n                        },\r\n                        this.$t(\"play\")\r\n                    ),h('span', {\r\n                        slot: 'content',\r\n                        style: {\r\n                            whiteSpace: 'normal'\r\n                        }\r\n                    }, '编辑')]),\r\n\r\n                  //设计 <Icon type=\"ios-desktop-outline\" />\r\n                  h('Tooltip',{props: {\r\n                      placement: 'top',\r\n                      transfer: true\r\n                    }},[h(\r\n                      \"Icon\",\r\n                      {\r\n                        props: {\r\n                          type: \"ios-desktop-outline\",\r\n                          size: \"\",\r\n                        },\r\n                        style: { marginRight: \"5px\",fontSize:'18px',cursor: 'pointer' },\r\n                        on: {\r\n                          click: () => {\r\n                            this.design(params.row);\r\n                          },\r\n                        },\r\n                      },\r\n                      this.$t(\"play\")\r\n                  ),h('span', {\r\n                    slot: 'content',\r\n                    style: {\r\n                      whiteSpace: 'normal'\r\n                    }\r\n                  }, '设计')]),\r\n\r\n\r\n                    //删除\r\n                    h('Tooltip',{props: {\r\n                            placement: 'top',\r\n                            transfer: true\r\n                        }},[h(\r\n                        \"Icon\",\r\n                        {\r\n                            props: {\r\n                                type: \"ios-trash-outline\",\r\n                                size: \"\",\r\n                            },\r\n                            style: { marginRight: \"5px\",fontSize:'18px',cursor: 'pointer' },\r\n                            on: {\r\n                                click: () => {\r\n                                    this.remove(params.row);\r\n                                },\r\n                            },\r\n                        },\r\n                        this.$t(\"play\")\r\n                    ),h('span', {\r\n                        slot: 'content',\r\n                        style: {\r\n                            whiteSpace: 'normal'\r\n                        }\r\n                    }, '删除')])\r\n                    ]);\r\n                }\r\n            }\r\n      ],\r\n      data: [],\r\n      total: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.getIvrWorkFlowList();\r\n      //this.initIVR();\r\n    },\r\n    initIVR(){\r\n      this.columnsSet=this.columns;\r\n      window.appid = WFCore.GetQuery(\"appid\");\r\n      //var iframeid = parent.tabiframeId();\r\n      window.iframeid = \"iframe_tabs_iframe_20170325-0926-5663-16a0-880e49a62e19\";\r\n      window.mainDialog = new RoadUI.Window();\r\n      window.dialog = window.mainDialog; //top.mainDialog;\r\n      if(window.setShowFlowData==undefined){\r\n        window.setShowFlowData = true; //是否启用右键菜单的查看属性菜单\r\n      }\r\n      window.wf_EnableStartEnd = true; //是否启用开始结束步骤，不启用则无法添加这两个步骤节点\r\n    },\r\n    changeSelect(e) {\r\n      this.selectList = e;\r\n      this.selectCount = e.length;\r\n      if(this.selectCount > 0){\r\n         this.openTip = true;\r\n      }else{\r\n         this.openTip = false;\r\n      }\r\n    },\r\n    handleSearch() {\r\n      this.selectList = \"\";\r\n      this.selectCount = 0;\r\n      this.searchForm.pagember = 1;\r\n      this.searchForm.pageSize = 10;\r\n      this.getIvrWorkFlowList();\r\n    },\r\n    handleReset() {\r\n      this.$refs.searchForm.resetFields();\r\n      this.searchForm.pageNumber = 1;\r\n      this.searchForm.pageSize = 10;\r\n    },\r\n    changePage(v) {\r\n      this.searchForm.pageNumber = v;\r\n      this.getIvrWorkFlowList();\r\n      this.clearSelectAll();\r\n    },\r\n    changePageSize(v) {\r\n      this.searchForm.pageSize = v;\r\n      this.getIvrWorkFlowList();\r\n    },\r\n    clearSelectAll() {\r\n      this.$refs.table.selectAll(false);\r\n    },\r\n    showSelect(e) {\r\n      this.selectList = e;\r\n      this.selectCount = e.length;\r\n    },\r\n    changeSort(e) {\r\n      this.searchForm.sort = e.key;\r\n      this.searchForm.order = e.order;\r\n      if (e.order == \"normal\") {\r\n        this.searchForm.order = \"\";\r\n      }\r\n      this.getIvrWorkFlowList();\r\n    },\r\n    getIvrWorkFlowList() {\r\n        this.loading = true;\r\n        var formdata={};\r\n        for(var i in this.searchForm){\r\n          if(this.searchForm[i] != null && this.searchForm[i] !=\"\"){\r\n            formdata[i] = this.searchForm[i]; \r\n          }\r\n        }\r\n        getAllIvrWorkFlowList(formdata).then(res => {\r\n          this.loading = false;\r\n          if (res.success) {\r\n            this.data = res.result.content;\r\n            this.total = res.result.totalPages;\r\n          }\r\n        });\r\n    },\r\n    addIvrWorkFlow(){\r\n          this.modalTitle=\"新增Ivr\";\r\n          this.ModalVisible = true;\r\n          this.modalType = 0;\r\n          this.ivrWorkFlowForm={};\r\n      // this.$router.push({path:'IVRVoiceDefault',params:{}})\r\n    },\r\n    edit(v) {\r\n      var formdata={};\r\n      for(let attr in v){\r\n        if(v[attr] != null && v[attr] !=\"\"){\r\n          formdata[attr] = v[attr]+\"\"; \r\n        }\r\n      }\r\n      this.modalType = 1;\r\n      this.ModalVisible = true;\r\n      this.modalTitle = \"编辑Ivr\";\r\n      this.ivrWorkFlowForm = formdata;\r\n    },\r\n    //设计\r\n    design(v){\r\n      this.IVRVoiceDefaultModalVisible=true;\r\n      this.IVRFormData=v;\r\n      window.appid=v.id;\r\n      window.iframeid = \"iframe_tabs_iframe_\"+Date.parse( new Date())+v.id;\r\n      window.mainDialog = new RoadUI.Window();\r\n      window.dialog = window.mainDialog; //top.mainDialog;\r\n      if(window.setShowFlowData==undefined){\r\n        window.setShowFlowData = true; //是否启用右键菜单的查看属性菜单\r\n      }\r\n      window.wf_EnableStartEnd = true; //是否启用开始结束步骤，不启用则无法添加这两个步骤节点\r\n    },\r\n    submitIvrWorkFlow() {\r\n          var formdata={};\r\n          if(!this.ivrWorkFlowForm.name) {\r\n            this.$Message.warning(\"流程名称不能为空!\");\r\n            return;\r\n          }\r\n          if(!this.ivrWorkFlowForm.wfversion) {\r\n            this.$Message.warning(\"版本号不能为空!\");\r\n            return;\r\n          }\r\n          for(var i in this.ivrWorkFlowForm){\r\n            if(this.ivrWorkFlowForm[i] != null && this.ivrWorkFlowForm[i] !=\"\"){\r\n                 formdata[i] = this.ivrWorkFlowForm[i];  \r\n            }\r\n          }\r\n          if (this.modalType == 0) {\r\n            // 添加\r\n            this.submitLoading = true;\r\n            addIvrWorkFlow(formdata).then(res => {\r\n              this.submitLoading = false;\r\n              if (res.success) {\r\n                this.$Message.success(\"操作成功\");\r\n                this.getIvrWorkFlowList();\r\n                this.ModalVisible = false;\r\n              }\r\n            });\r\n          } else {\r\n            this.submitLoading = true;\r\n            editIvrWorkFlow(formdata).then(res => {\r\n              this.submitLoading = false;\r\n              if (res.success) {\r\n                this.$Message.success(\"操作成功\");\r\n                this.getIvrWorkFlowList();\r\n                this.ModalVisible = false;\r\n              }\r\n            });\r\n        }\r\n    },\r\n    remove(v) {\r\n      this.$Modal.confirm({\r\n        title: \"确认删除\",\r\n        // 记得确认修改此处\r\n        content: \"您确认要删除 \" + v.name + \" ?\",\r\n        loading: true,\r\n        onOk: () => {\r\n          // 删除\r\n          deleteIvrWorkFlow({ ids: v.id }).then((res) => {\r\n            this.$Modal.remove();\r\n            if (res.success) {\r\n              this.$Message.success(\"操作成功\");\r\n              this.getIvrWorkFlowList();\r\n              this.selectCount = 0;\r\n            }\r\n          });\r\n        },\r\n      });\r\n    },\r\n    cancelIvrWorkFlow() {\r\n      this.ModalVisible = false;\r\n\r\n    }\r\n  },\r\n  mounted() {\r\n    // 计算高度\r\n    this.height = Number(document.documentElement.clientHeight - 230);\r\n    this.init();\r\n  }\r\n};\r\n</script>"]}]}