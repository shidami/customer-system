;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAijBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"extNumber.vue","sourceRoot":"src/views/sys/ext-number","sourcesContent":["<template>\r\n  <div class=\"search\">\r\n    <Card>\r\n      <Row class=\"operation\">\r\n        <Button @click=\"modifyRelayGroup\" type=\"primary\" icon=\"md-create\">批量设置主叫号码</Button>\r\n        <Button @click=\"modifyPassword\" icon=\"md-create\">批量修改密码</Button>\r\n        <Button @click=\"getDataList\" icon=\"md-refresh\">刷新</Button>\r\n        <Button type=\"dashed\" @click=\"openTip = !openTip\">{{\r\n          openTip ? \"关闭提示\" : \"开启提示\"\r\n        }}</Button>\r\n      </Row>\r\n      <Row v-show=\"openTip && selectCount > 0\">\r\n        <Alert show-icon>\r\n          已选择\r\n          <span class=\"select-count\">{{ selectCount }}</span> 项\r\n          <a class=\"select-clear\" @click=\"clearSelectAll\">清空</a>\r\n        </Alert>\r\n      </Row>\r\n      <Row>\r\n        <Table\r\n          :loading=\"loading\"\r\n          border\r\n          :columns=\"columns\"\r\n          :data=\"data\"\r\n          ref=\"table\"\r\n          sortable=\"custom\"\r\n          @on-sort-change=\"changeSort\"\r\n          @on-selection-change=\"changeSelect\"\r\n        ></Table>\r\n      </Row>\r\n      <Row type=\"flex\" justify=\"end\" class=\"page\">\r\n        <Page\r\n          :current=\"searchForm.pageNumber\"\r\n          :total=\"total\"\r\n          :page-size=\"searchForm.pageSize\"\r\n          @on-change=\"changePage\"\r\n          @on-page-size-change=\"changePageSize\"\r\n          :page-size-opts=\"[10, 20, 50]\"\r\n          size=\"small\"\r\n          show-total\r\n          show-elevator\r\n          show-sizer\r\n        ></Page>\r\n      </Row>\r\n    </Card>\r\n\r\n    <!-- 编辑密码 -->\r\n    <Modal :title=\"modalTitle\" v-model=\"extModalVisible\" :mask-closable=\"false\" :width=\"500\">\r\n      <Form ref=\"extsForm\" :model=\"extsForm\" :label-width=\"110\">\r\n        <FormItem label=\"密码\" prop=\"passWords\">\r\n          <Input v-model=\"extsForm.passWords\"  />\r\n        </FormItem>\r\n      </Form>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"cancelExtNumber\">取消</Button>\r\n        <Button type=\"primary\" :loading=\"submitLoading\" @click=\"submitExtNumber(1)\">提交</Button>\r\n      </div>\r\n    </Modal>\r\n\r\n    <!-- 编辑主备叫号码 -->\r\n    <Modal :title=\"relayGroupModalTitle\" v-model=\"relayGroupModalVisible\" :mask-closable=\"false\" :width=\"500\">\r\n      <Form ref=\"extsForm\" :model=\"extsForm\" :label-width=\"110\">\r\n        <FormItem label=\"中继组\" prop=\"relayGroupNo\">\r\n          <Select v-model=\"extsForm.relayGroupNo\" @on-change=\"getRelayPhoneAll()\" placeholder=\"请选择\">\r\n            <Option v-for=\"(item, index) in relayGroupListItems\" :key=\"index\" :value=\"item.id\"  >{{ item.relayGroupName }}</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"主键号码\" prop=\"caller\">\r\n          <Select v-model=\"extsForm.caller\" placeholder=\"请选择\">\r\n            <Option v-for=\"(item, index) in relayPhoneListItems\" :key=\"index\" :value=\"item.relayPhoneNumber\"  >{{ item.relayPhoneNumber }}</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"备用中继组\" prop=\"slaveRelayGroupNo\">\r\n          <Select v-model=\"extsForm.slaveRelayGroupNo\" @on-change=\"getSlaveRelayPhoneAll()\" placeholder=\"请选择\">\r\n            <Option v-for=\"(item, index) in slaveRelayGroupListItems\" :key=\"index\" :value=\"item.id\"  >{{ item.relayGroupName }}</Option>\r\n          </Select>\r\n        </FormItem>\r\n        <FormItem label=\"备用主键号码\" prop=\"slaveCaller\">\r\n          <Select v-model=\"extsForm.slaveCaller\" placeholder=\"请选择\">\r\n            <Option v-for=\"(item, index) in slaveRelayPhoneListItems\" :key=\"index\" :value=\"item.relayPhoneNumber\"  >{{ item.relayPhoneNumber }}</Option>\r\n          </Select>\r\n        </FormItem>\r\n      </Form>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"cancelRelayGroup\">取消</Button>\r\n        <Button type=\"primary\" :loading=\"submitLoading\" @click=\"submitExtNumber(2)\">提交</Button>\r\n      </div>\r\n    </Modal>\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n// 根据你的实际请求api.js位置路径修改\r\nimport {\r\n  getAllExtNumber,\r\n  getExtNumberByCondition,\r\n  editExtNumberList,\r\n  getRelayGroupList,\r\n  getRelayPhoneList\r\n} from \"@/api/index\";\r\nexport default {\r\n  name: \"ext-number\",\r\n  data() {\r\n    return {\r\n      openTip: true, // 显示提示\r\n      formData: {},\r\n      loading: true, // 表单加载状态\r\n      modalTitle: \"\",\r\n      relayGroupModalTitle: \"\",\r\n      modalType: 0,\r\n      extModalVisible: false,\r\n      relayGroupModalVisible:false,\r\n      submitLoading: false,\r\n      relayGroupListItems:[],\r\n      relayPhoneListItems:[],  \r\n      slaveRelayGroupListItems:[],\r\n      slaveRelayPhoneListItems:[],  \r\n      searchForm: {\r\n        // 搜索框初始化对象\r\n        pageNumber: 1, // 当前页数\r\n        pageSize: 10, // 页面大小\r\n        sort: \"ext\", // 默认排序字段\r\n        order: \"asc\", // 默认排序方式\r\n      },\r\n      extsForm: {\r\n        passWords:\"\",\r\n        relayGroupNo:\"\",\r\n        caller:\"\",\r\n        ctiNo:\"\",\r\n        slaveRelayGroupNo:\"\",\r\n        slaveCaller:\"\",\r\n        slavectiNo:\"\"\r\n      },\r\n      extFormValidate: {\r\n        beginExt: [\r\n          {\r\n            required: true,\r\n            type: \"integer\",\r\n            message: \"请输入起始分机号\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        number: [\r\n          {\r\n            required: true,\r\n            type: \"integer\",\r\n            message: \"请输入分机数量\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n        obEnabled: [\r\n          {\r\n            required: true,\r\n            type: \"integer\",\r\n            message: \"请选择呼叫模式\",\r\n            trigger: \"blur\",\r\n          },\r\n        ],\r\n      },\r\n      selectList: [], // 多选数据\r\n      selectCount: 0, // 多选计数\r\n      columns: [\r\n        // 表头\r\n        {\r\n          type: \"selection\",\r\n          width: 60,\r\n          align: \"center\",\r\n        },\r\n        {\r\n          title: \"分机号码\",\r\n          key: \"ext\",\r\n          minWidth: 120,\r\n          sortable: true,\r\n        },\r\n        {\r\n          title: \"分机密码\",\r\n          key: \"password\",\r\n          width: 120,\r\n          align: \"center\",\r\n        },\r\n        // {\r\n        //   title: \"主CTI\",\r\n        //   key: \"masterCti\",\r\n        //   minWidth: 120,\r\n        //   sortable: false,\r\n        //   hidden: true\r\n        // },\r\n        // {\r\n        //   title: \"主中继组编号\",\r\n        //   key: \"masterRelayGroupNo\",\r\n        //   minWidth: 120,\r\n        //   sortable: false,\r\n        //   hidden: true\r\n        // },\r\n        {\r\n          title: \"主叫号码\",\r\n          key: \"masterCaller\",\r\n          minWidth: 120,\r\n          sortable: false,\r\n        },\r\n        // {\r\n        //   title: \"备CTI\",\r\n        //   key: \"slaveCti\",\r\n        //   minWidth: 120,\r\n        //   sortable: false,\r\n        //   hidden: true\r\n        // },\r\n        // {\r\n        //   title: \"备中继组编号\",\r\n        //   key: \"slaveRelayGroupNo\",\r\n        //   minWidth: 120,\r\n        //   sortable: false,\r\n        //   hidden: true\r\n        // },\r\n        // {\r\n        //   title: \"备用主叫号码\",\r\n        //   key: \"slaveCaller\",\r\n        //   minWidth: 120,\r\n        //   sortable: false,\r\n        //   hidden: true\r\n        // },\r\n        {\r\n          title: \"绑定IP\",\r\n          key: \"ipAddress\",\r\n          minWidth: 120,\r\n          sortable: false,\r\n        },\r\n        {\r\n          title: \"呼叫模式\",\r\n          key: \"obEnabled\",\r\n          minWidth: 130,\r\n          sortable: false,\r\n          render: (h, params) => {\r\n            let location = \"\",\r\n              color = \"\";\r\n            if (params.row.obEnabled == 0) {\r\n              location = \"呼入呼出\";\r\n              color = \"green\";\r\n            } else if (params.row.obEnabled == -1) {\r\n              location = \"仅呼入\";\r\n              color = \"orange\";\r\n            }\r\n            return h(\"div\", [\r\n              h(\r\n                \"Tag\",\r\n                {\r\n                  props: {\r\n                    color: color,\r\n                  },\r\n                },\r\n                location\r\n              ),\r\n            ]);\r\n          },\r\n          filters: [\r\n            {\r\n              label: \"呼入呼出\",\r\n              value: 1,\r\n            },\r\n            {\r\n              label: \"仅呼入\",\r\n              value: 0,\r\n            },\r\n          ],\r\n          filterMultiple: false,\r\n          filterMethod(value, row) {\r\n            if (value == 0) {\r\n              return row.obEnabled == 0;\r\n            } else if (value == 1) {\r\n              return row.obEnabled == 1;\r\n            }\r\n          },\r\n        },\r\n        {\r\n          title: \"分机状态\",\r\n          key: \"enabled\",\r\n          minWidth: 130,\r\n          sortable: false,\r\n          render: (h, params) => {\r\n            if (params.row.enabled == 0) {\r\n              return h(\"div\", [\r\n                h(\"Badge\", {\r\n                  props: {\r\n                    status: \"success\",\r\n                    text: \"启用\",\r\n                  },\r\n                }),\r\n              ]);\r\n            } else if (params.row.enabled == -1) {\r\n              return h(\"div\", [\r\n                h(\"Badge\", {\r\n                  props: {\r\n                    status: \"error\",\r\n                    text: \"停用\",\r\n                  },\r\n                }),\r\n              ]);\r\n            }\r\n          },\r\n          filters: [\r\n            {\r\n              label: \"启用\",\r\n              value: 0,\r\n            },\r\n            {\r\n              label: \"停用\",\r\n              value: -1,\r\n            },\r\n          ],\r\n          filterMultiple: false,\r\n          filterMethod(value, row) {\r\n            if (value == 0) {\r\n              return row.enabled == 0;\r\n            } else if (value == -1) {\r\n              return row.enabled == -1;\r\n            }\r\n          },\r\n        },\r\n        {\r\n          title: \"说明\",\r\n          key: \"remark\",\r\n          minWidth: 300,\r\n          sortable: false,\r\n        },\r\n        {\r\n          title: \"操作\",\r\n          key: \"action\",\r\n          align: \"center\",\r\n          width: 160,\r\n          fixed: \"right\",\r\n          render: (h, params) => {\r\n            let enableOrDisable = \"\";\r\n            return h(\"div\", [\r\n              enableOrDisable,\r\n              h(\r\n                \"Button\",\r\n                {\r\n                  props: {\r\n                    type: \"primary\",\r\n                    size: \"small\",\r\n                  },\r\n                  style: {\r\n                    marginRight: \"5px\",\r\n                  },\r\n                  on: {\r\n                    click: () => {\r\n                      this.edit(params.row);\r\n                    },\r\n                  },\r\n                },\r\n                \"绑定\"\r\n              ),\r\n            ]);\r\n          },\r\n        },\r\n      ],\r\n      data: [], // 表单数据\r\n      pageNumber: 1, // 当前页数\r\n      pageSize: 10, // 页面大小\r\n      total: 0, // 表单数据总数\r\n    };\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.getDataList();\r\n    },\r\n    submited() {\r\n      this.getDataList();\r\n    },\r\n    changePage(v) {\r\n      this.searchForm.pageNumber = v;\r\n      this.getDataList();\r\n      this.clearSelectAll();\r\n    },\r\n    changePageSize(v) {\r\n      this.searchForm.pageSize = v;\r\n      this.getDataList();\r\n    },\r\n    changeSort(e) {\r\n      this.searchForm.sort = e.key;\r\n      this.searchForm.order = e.order;\r\n      if (e.order === \"normal\") {\r\n        this.searchForm.order = \"\";\r\n      }\r\n      this.getDataList();\r\n    },\r\n    clearSelectAll() {\r\n      this.$refs.table.selectAll(false);\r\n    },\r\n    changeSelect(e) {\r\n      this.selectList = e;\r\n      this.selectCount = e.length;\r\n    },\r\n    getDataList() {\r\n      this.loading = true;\r\n      getExtNumberByCondition(this.searchForm).then((res) => {\r\n        this.loading = false;\r\n        if (res.success) {\r\n          this.data = res.result.content;\r\n          this.total = res.result.totalElements;\r\n        }\r\n      });\r\n    },\r\n    showDetail(v) {\r\n      // 转换null为\"\"\r\n      for (let attr in v) {\r\n        if (v[attr] == null) {\r\n          v[attr] = \"\";\r\n        }\r\n      }\r\n      let str = JSON.stringify(v);\r\n      let data = JSON.parse(str);\r\n      // this.extForm = data;\r\n      // this.showExt = true;\r\n    },\r\n    modifyPassword(){\r\n      this.extModalVisible = true;\r\n      this.modalTitle = \"批量修改密码\";\r\n      if(this.selectList.length <=0){\r\n            this.$Message.warning(\"请选择需要操作的数据\");\r\n            return;\r\n      }\r\n    },\r\n    modifyRelayGroup(){\r\n      this.relayGroupModalVisible = true;\r\n      this.relayGroupModalTitle = \"批量设置主叫号码\";\r\n      if(this.selectList.length <=0){\r\n            this.$Message.warning(\"请选择需要操作的数据\");\r\n            return;\r\n      }\r\n      this.extsForm = {};\r\n      this.relayGroupListItems=[];\r\n      this.relayPhoneListItems=[];  \r\n      this.slaveRelayGroupListItems=[];\r\n      this.slaveRelayPhoneListItems=[];  \r\n      this.getRelayGroupAll();\r\n    },\r\n    cancelRelayGroup(){\r\n      this.relayGroupModalVisible = false;\r\n      this.extsForm = {};\r\n      this.relayGroupListItems=[];\r\n      this.relayPhoneListItems=[];  \r\n      this.slaveRelayGroupListItems=[];\r\n      this.slaveRelayPhoneListItems=[];  \r\n    },\r\n    cancelExtNumber(){\r\n      this.extModalVisible = false;\r\n      this.extsForm = {};\r\n    },\r\n    submitExtNumber(type){\r\n          if(type == 1){\r\n            if(!this.extsForm.passWords){\r\n              this.$Message.warning(\"密码不能为空\");\r\n              return;\r\n            }\r\n          }else{\r\n               if(!this.extsForm.relayGroupNo){\r\n                  this.$Message.warning(\"中继组不能为空\");\r\n                  return;\r\n               }\r\n               if(!this.extsForm.caller){\r\n                  this.$Message.warning(\"主叫号码不能为空\");\r\n                  return;\r\n               }\r\n               if(!this.extsForm.slaveRelayGroupNo){\r\n                  this.$Message.warning(\"备份中继组不能为空\");\r\n                  return;\r\n               }\r\n               if(!this.extsForm.slaveCaller){\r\n                  this.$Message.warning(\"备份主叫号码不能为空\");\r\n                  return;\r\n               }\r\n          }\r\n          let ids = \"\";\r\n          this.selectList.forEach(function (e) {\r\n            ids += e.id + \",\";\r\n          });\r\n          ids = ids.substring(0, ids.length - 1);\r\n          var formDate = {};\r\n          for(var i in this.extsForm){\r\n            if(this.extsForm[i] != null && this.extsForm[i] !=\"\"){\r\n              formDate[i] = this.extsForm[i]; \r\n            }\r\n          }\r\n          formDate[\"ids\"] = ids;\r\n          this.submitLoading = true;\r\n          editExtNumberList(formDate).then(res => {\r\n            this.submitLoading = false;\r\n            if (res.success) {\r\n              this.extsForm = {};\r\n              this.$Message.success(\"操作成功\");\r\n              this.getDataList();\r\n              this.extModalVisible = false;\r\n              this.relayGroupModalVisible = false;\r\n            }\r\n          });\r\n    },\r\n    getRelayGroupAll() { //获取中继组\r\n      getRelayGroupList({}).then((res) => {\r\n        if (res.success) {\r\n          this.relayGroupListItems = res.result.list;\r\n        }\r\n      });\r\n    },\r\n    getRelayPhoneAll() { //获取中继号码\r\n      var relayGroupId = \"\";\r\n      if(this.extsForm.relayGroupNo){\r\n          relayGroupId = this.extsForm.relayGroupNo;\r\n          var obj = {};\r\n          obj = this.relayGroupListItems.find(item =>{\r\n              return item.id == relayGroupId; \r\n          });\r\n          this.extsForm.ctiNo = obj.ctiManageId;\r\n      }\r\n      this.extsForm.caller=\"\";\r\n      this.extsForm.slaveRelayGroupNo=\"\";\r\n      this.extsForm.slaveCaller=\"\";\r\n      this.extsForm.slaveCtiNo=\"\";\r\n      getRelayPhoneList({blackMark:1,workMode:1,relayGroupId:relayGroupId}).then((res) => {\r\n        if (res.success) {\r\n          this.relayPhoneListItems = [];\r\n          this.relayPhoneListItems = res.result.list;\r\n        }\r\n      });\r\n\r\n      getRelayGroupList({prepareGroupId:relayGroupId}).then((res) => {\r\n        if (res.success) {\r\n          this.slaveRelayGroupListItems = [];\r\n          this.slaveRelayGroupListItems = res.result.list;\r\n        }\r\n      });\r\n\r\n    },\r\n    getSlaveRelayPhoneAll() { //获取中继号码\r\n      var relayGroupId = \"\";\r\n      if(this.extsForm.slaveRelayGroupNo){\r\n          relayGroupId = this.extsForm.slaveRelayGroupNo;\r\n          var obj = {};\r\n          obj = this.slaveRelayGroupListItems.find(item =>{\r\n              return item.id == relayGroupId; \r\n          });\r\n          this.extsForm.slaveCtiNo = obj.ctiManageId;\r\n      }\r\n      getRelayPhoneList({blackMark:1,workMode:1,relayGroupId:relayGroupId}).then((res) => {\r\n        if (res.success) {\r\n          this.slaveRelayPhoneListItems = [];\r\n          this.slaveRelayPhoneListItems = res.result.list;\r\n        }\r\n      });\r\n    }\r\n  },\r\n  mounted() {\r\n    this.init();\r\n  },\r\n  props: {\r\n    tenantId: String,\r\n  },\r\n};\r\n</script>\r\n<style lang=\"less\">\r\n// 建议引入通用样式 具体路径自行修改 可删除下面样式代码\r\n@import \"../../../styles/table-common.less\";\r\n.search {\r\n  .operation {\r\n    margin-bottom: 2vh;\r\n  }\r\n  .select-count {\r\n    font-weight: 600;\r\n    color: #40a9ff;\r\n  }\r\n  .select-clear {\r\n    margin-left: 10px;\r\n  }\r\n  .page {\r\n    margin-top: 2vh;\r\n  }\r\n  .drop-down {\r\n    margin-left: 5px;\r\n  }\r\n}\r\n</style>"]}]}