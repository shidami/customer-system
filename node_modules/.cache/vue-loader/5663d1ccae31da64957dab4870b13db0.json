ACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"execriseForm.vue","sourceRoot":"src/views","sourcesContent":["<style lang=\"less\">\r\n</style>\r\n<template>\r\n  <div class=\"execriseForm\">\r\n    <Card>\r\n      <Row class=\"operation\">\r\n        <Button @click=\"add\" type=\"primary\" icon=\"md-add\">添加</Button>\r\n        <Button @click=\"delAll\" icon=\"md-trash\">批量删除</Button>\r\n        <Button @click=\"getDataList\" icon=\"md-refresh\">刷新</Button>\r\n        <Button type=\"dashed\" @click=\"openTip = !openTip\">{{\r\n          openTip ? \"关闭提示\" : \"开启提示\"\r\n        }}</Button>\r\n      </Row>\r\n      <Row v-show=\"openTip && selectCount > 0\">\r\n        <Alert show-icon>\r\n          已选择\r\n          <span class=\"select-count\">{{ selectCount }}</span> 项\r\n          <a class=\"select-clear\" @click=\"clearSelectAll\">清空</a>\r\n        </Alert>\r\n      </Row>\r\n      <Row>\r\n        <Table\r\n          :loading=\"loading\"\r\n          border\r\n          :columns=\"columns\"\r\n          :data=\"data\"\r\n          ref=\"table\"\r\n          sortable=\"custom\"\r\n          @on-sort-change=\"changeSort\"\r\n          @on-selection-change=\"changeSelect\"\r\n        ></Table>\r\n      </Row>\r\n      <Row type=\"flex\" justify=\"end\" class=\"page\">\r\n        <Page\r\n          :current=\"searchForm.pageNumber\"\r\n          :total=\"total\"\r\n          :page-size=\"searchForm.pageSize\"\r\n          @on-change=\"changePage\"\r\n          @on-page-size-change=\"changePageSize\"\r\n          :page-size-opts=\"[10, 20, 50]\"\r\n          size=\"small\"\r\n          show-total\r\n          show-elevator\r\n          show-sizer\r\n        ></Page>\r\n      </Row>\r\n    </Card>\r\n  </div>\r\n</template>\r\n<script>\r\nimport { getTenantList, disableTenant, enableTenant } from \"@/api/index\";\r\nexport default {\r\n  name: \"execrise-Form\",\r\n  data() {\r\n    return {\r\n      openTip: \"\",\r\n      selectCount: NaN,\r\n      loading: false,\r\n      columns: [\r\n        {\r\n          type: \"selection\",\r\n          width: 60,\r\n          align: \"center\",\r\n          fixed: \"left\",\r\n        },\r\n        {\r\n          title: \"#\",\r\n          key: \"#\",\r\n          fixed: \"left\",\r\n          width: 60,\r\n          type: \"index\",\r\n        },\r\n        {\r\n          title: \"租户名称\",\r\n          key: \"title\",\r\n          fixed: \"left\",\r\n          width: 150,\r\n        },\r\n        {\r\n          title: \"租户编码\",\r\n          key: \"code\",\r\n          fixed: \"left\",\r\n          width: 100,\r\n        },\r\n        {\r\n          title: \"邮箱\",\r\n          key: \"email\",\r\n          width: 300,\r\n        },\r\n        {\r\n          title: \"联系电话\",\r\n          key: \"mobile\",\r\n          width: 300,\r\n        },\r\n        {\r\n          title: \"租户类型\",\r\n          key: \"category\",\r\n          width: 120,\r\n          filters:[\r\n            {\r\n              label: \"正式租户\",\r\n              value: 1,\r\n            },\r\n            {\r\n              label: \"试用租户\",\r\n              value: 0,\r\n            }\r\n          ],\r\n          filterMultiple: false,\r\n\r\n          filterMethod(value, row) {\r\n            if (value == 0) {\r\n              return row.type == 0;\r\n            } else if (value == 1) {\r\n              return row.type == 1;\r\n            }\r\n          },\r\n          render: (h, params) => {\r\n            let color = \"\";\r\n            let tenant = \"\";\r\n            if (params.row.type == 1) {\r\n              color = \"green\";\r\n              tenant = \"适用租户\";\r\n            } else {\r\n              color = \"orange\";\r\n              tenant = \"正式租户\";\r\n            }\r\n            return h(\"div\", [\r\n              h(\r\n                \"tag\",\r\n                {\r\n                  props: {\r\n                    color: color,\r\n                    size: \"small\",\r\n                  },\r\n                },\r\n                tenant\r\n              ),\r\n            ]);\r\n          },\r\n        },\r\n        {\r\n          title: \"租户状态\",\r\n          key: \"status\",\r\n          width: 120,\r\n          render: (h, params) => {\r\n            let status = \"\";\r\n            let text = \"\";\r\n            if (params.row.status == 0) {\r\n              status = \"success\";\r\n              text = \"正常启用\";\r\n            } else {\r\n              status = \"error\";\r\n              text = \"禁用\";\r\n            }\r\n            return h(\"div\", [\r\n              h(\"badge\", {\r\n                props: {\r\n                  status: status,\r\n                  text: text,\r\n                },\r\n              }),\r\n            ]);\r\n          },\r\n        },\r\n        {\r\n          title: \"到期时间\",\r\n          key: \"experience_date\",\r\n          width: 180,\r\n        },\r\n        {\r\n          title: \"操作\",\r\n          key: \"operation\",\r\n          width: 200,\r\n          align: \"center\",\r\n          fixed: \"right\",\r\n          render: (h, params) => {\r\n            let enableOrDisable = \"\";\r\n            if (params.row.status == 0) {\r\n              enableOrDisable = h(\r\n                \"Button\",\r\n                {\r\n                  props: {\r\n                    size: \"small\",\r\n                  },\r\n                  on: {\r\n                    click: () => {\r\n                      this.disable(params.row);\r\n                    },\r\n                  },\r\n                },\r\n                \"禁用\"\r\n              );\r\n            } else {\r\n              enableOrDisable = h(\r\n                \"Button\",\r\n                {\r\n                  props: {\r\n                    type: \"success\",\r\n                    size: \"small\",\r\n                  },\r\n                  on: {\r\n                    click: () => {\r\n                      this.enable(params.row);\r\n                    },\r\n                  },\r\n                },\r\n                \"启用\"\r\n              );\r\n            }\r\n            return h(\"div\", [\r\n              h(\r\n                \"Button\",\r\n                {\r\n                  props: {\r\n                    type: \"primary\",\r\n                    size: \"small\",\r\n                  },\r\n                  style: {\r\n                    marginRight: \"5px\",\r\n                  },\r\n                  on: {\r\n                    click: () => {\r\n                      this.show(params.index);\r\n                    },\r\n                  },\r\n                },\r\n                \"编辑\"\r\n              ),\r\n              enableOrDisable,\r\n              h(\r\n                \"Button\",\r\n                {\r\n                  props: {\r\n                    type: \"error\",\r\n                    size: \"small\",\r\n                  },\r\n                  on: {\r\n                    click: () => {\r\n                      this.remove(params.index);\r\n                    },\r\n                  },\r\n                },\r\n                \"删除\"\r\n              ),\r\n            ]);\r\n          },\r\n        },\r\n      ],\r\n      data: [],\r\n      searchForm: {\r\n        pageNumber: NaN,\r\n        pageSize: NaN,\r\n      },\r\n      total: NaN,\r\n    };\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.getDataList();\r\n    },\r\n    add() {},\r\n    delAll() {},\r\n    getDataList() {\r\n      getTenantList().then((res) => {\r\n        let data = res.result.content;\r\n        console.log(\"111111111111111111111\", data);\r\n        this.data = data;\r\n      });\r\n    },\r\n    clearSelectAll() {},\r\n    changeSort() {},\r\n    changeSelect() {},\r\n    changePage() {},\r\n    changePageSize() {},\r\n    enable(v) {\r\n      this.$Modal.confirm({\r\n        title: \"确认启用\",\r\n        content: \"您确认要启用租户 \" + v.title + \" ?\",\r\n        loading: true,\r\n        onOk: () => {\r\n          enableTenant(v.id).then((res) => {\r\n            this.$Modal.remove();\r\n            if (res.success) {\r\n              this.$Message.success(\"操作成功\");\r\n              this.getDataList();\r\n            }\r\n          });\r\n        },\r\n      });\r\n    },\r\n    disable(v) {\r\n      this.$Modal.confirm({\r\n        title: \"确认禁用\",\r\n        content: \"您确认要禁用租户 \" + v.title + \" ?\",\r\n        loading: true,\r\n        onOk: () => {\r\n          disableTenant(v.id).then((res) => {\r\n            this.$Modal.remove();\r\n            if (res.success) {\r\n              this.$Message.success(\"操作成功\");\r\n              this.getDataList();\r\n            }\r\n          });\r\n        },\r\n      });\r\n    },\r\n  },\r\n  mounted() {\r\n    this.init();\r\n  },\r\n  watch: {},\r\n};\r\n</script>"]}]}