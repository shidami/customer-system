border>{{item.title}}</Checkbox>\r\n          </CheckboxGroup>\r\n        </FormItem>\r\n        <FormItem label=\"联络结果\" prop=\"contactResult\">\r\n          <RadioGroup v-model=\"formData.contactResult\">\r\n            <Radio v-for=\"item in $store.state.dict.contactResults\" class=\"mt-5 mb-5\" :label=\"item.value\" border>{{item.title}}</Radio>\r\n          </RadioGroup>\r\n        </FormItem>\r\n      <FormItem label=\"跟进结果\" prop=\"result\">\r\n        <RadioGroup v-model=\"formData.result\">\r\n          <Radio label=\"转外部门跟进\" border></Radio>\r\n          <Radio label=\"未联系客户直接制单\" border></Radio>\r\n        </RadioGroup>\r\n      </FormItem>\r\n    </Form>\r\n    <div slot=\"footer\">\r\n      <Button type=\"text\" @click=\"visiable=false\">取消</Button>\r\n      <Button type=\"primary\" :loading=\"submitLoading\" @click=\"submitAdd\">提交</Button>\r\n    </div>\r\n  </Modal>\r\n</template>\r\n<script>\r\nimport {\r\n  editHandle,    //处理按钮线下受理跟进\r\n} from \"@/api/index\";\r\nexport default {\r\n    data() {\r\n      return {\r\n        visiable:false,\r\n        submitLoading:false,\r\n        formData:{\r\n          contactItems:[],\r\n          contactResult:'',\r\n          result:'',\r\n        },\r\n        ruleValidate: {\r\n          contactItems: [{required:true,type:'array',min: 1, message: '请至少选择一项',trigger: 'change'}],\r\n          contactResult:[{required: true, message: '请至少选择一项', trigger: 'change' }],\r\n          result:[{required: true, message: '请至少选择一项', trigger: 'change' }],\r\n        }\r\n      }\r\n  },\r\n  methods: {\r\n    submitAdd(){\r\n      this.$refs.form.validate((valid) => {\r\n        if (valid) {\r\n          this.formData.contactItems=this.formData.contactItems.toString()\r\n          this.submitLoading=true\r\n          editHandle(this.formData).then(res => {\r\n            this.submitLoading=false\r\n            if (res.success) {\r\n              this.$Message.success({\r\n                background: true,\r\n                content: '提交成功'\r\n              });\r\n              this.handleReset()\r\n              this.visiable=false\r\n              this.$parent.getDataList()\r\n            }else{\r\n              this.$Message.error({\r\n                background: true,\r\n                content: res.message\r\n              });\r\n            }\r\n          });\r\n        }\r\n      })\r\n    },\r\n    handleReset() {\r\n      this.$refs.form.resetFields();\r\n    },\r\n  }\r\n}\r\n</script>\r\n"]}]}