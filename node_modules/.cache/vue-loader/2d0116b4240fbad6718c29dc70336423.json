{"remainingRequest":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\src\\views\\callcenter\\sysRoute.vue?vue&type=style&index=0&lang=less&","dependencies":[{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\src\\views\\callcenter\\sysRoute.vue","mtime":1614579763231},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\less-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"C:\\Users\\Administrator\\Documents\\shimin\\current project\\xrcc-front\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:DQpAaW1wb3J0ICIuLi8uLi9zdHlsZXMvdGFibGUtY29tbW9uLmxlc3MiOw0K"},{"version":3,"sources":["sysRoute.vue"],"names":[],"mappings":";AACA","file":"sysRoute.vue","sourceRoot":"src/views/callcenter","sourcesContent":["<style lang=\"less\">\r\n@import \"../../styles/table-common.less\";\r\n</style>\r\n<template>\r\n  <div class=\"search\">\r\n    <Card>\r\n      <Row>\r\n        <Form ref=\"searchForm\" :model=\"searchForm\" inline :label-width=\"70\">\r\n          <Row>\r\n          <Form-item style=\"margin-left:-35px;\" class=\"br\">\r\n            <!-- <Button @click.native=\"handleSearch\" type=\"primary\" icon=\"ios-search\">搜索</Button>\r\n            <Button @click=\"handleReset\">重置</Button> -->\r\n            <Button @click=\"addSysRoute\" type=\"primary\" icon=\"md-add\">添加路由</Button>\r\n          </Form-item>\r\n          </Row>\r\n        </Form>\r\n      </Row>\r\n      <Row v-show=\"openTip && selectCount > 0\">\r\n        <Alert show-icon>\r\n          已选择\r\n          <span class=\"select-count\">{{selectCount}}</span> 项\r\n          <a class=\"select-clear\" @click=\"clearSelectAll\">清空</a>\r\n        </Alert>\r\n      </Row>\r\n      <Row>\r\n        <Table\r\n          :loading=\"loading\"\r\n          :columns=\"columns\"\r\n          :data=\"data\"\r\n          sortable=\"custom\"\r\n          @on-sort-change=\"changeSort\"\r\n          @on-selection-change=\"showSelect\"\r\n          ref=\"table\"\r\n        ></Table>\r\n      </Row>\r\n      <Row type=\"flex\" justify=\"end\" class=\"page\">\r\n        <Page\r\n          :current=\"searchForm.pageNumber\"\r\n          :total=\"total\"\r\n          :page-size=\"searchForm.pageSize\"\r\n          @on-change=\"changePage\"\r\n          @on-page-size-change=\"changePageSize\"\r\n          :page-size-opts=\"[10,20,50]\"\r\n          size=\"small\"\r\n          show-total\r\n          show-elevator\r\n          show-sizer\r\n        ></Page>\r\n      </Row>\r\n    </Card>\r\n\r\n\r\n    <!-- 新增编辑 -->\r\n    <Modal :title=\"modalTitle\" v-model=\"ModalVisible\" :mask-closable=\"false\" :width=\"500\">\r\n      <Form ref=\"sysRouteForm\" :model=\"sysRouteForm\" :label-width=\"80\">\r\n        <FormItem label=\"优先级\" prop=\"sortCode\">\r\n          <Input v-model=\"sysRouteForm.sortCode\"  />\r\n        </FormItem>\r\n        <FormItem label=\"备注\" prop=\"remark\">\r\n          <Input type=\"textarea\" v-model=\"sysRouteForm.remark\" :rows=\"4\" />\r\n        </FormItem>\r\n      </Form>\r\n      <div slot=\"footer\">\r\n        <Button type=\"text\" @click=\"cancelSysRoute\">取消</Button>\r\n        <Button type=\"primary\" :loading=\"submitLoading\" @click=\"submitSysRoute\">提交</Button>\r\n      </div>\r\n    </Modal>\r\n\r\n\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  getAllBySysRouteList,      //获取当前公司路由规则数据\r\n  getSysRouteAllByPage,      //获取列表分页数据\r\n  getRelayPhoneList,         //获取当前公司中继号码\r\n  addSysRoute, \r\n  editSysRoute, \r\n  deleteSysRoute,\r\n  getAllByIvrRobot     //获取机器人信息\r\n} from \"@/api/index\";\r\nexport default {\r\n  name: \"sysroute-manage\",\r\n  data() {\r\n    return {\r\n      height: 510,\r\n      openTip: true,\r\n      loading: false,\r\n      selectCount: 0,\r\n      selectList: [],\r\n      modalTitle:\"\", \r\n      ModalVisible:false,\r\n      modalType: 0,\r\n      relayPhoneListItems:[],\r\n      robotListItems:[],\r\n      sysRouteForm:{    \r\n        sortCode:\"\",\r\n        remark:\"\"\r\n      },\r\n      searchForm: {\r\n        pageNumber: 1,\r\n        pageSize: 10,\r\n        sort: \"Id\",\r\n        order: \"desc\"\r\n      },\r\n      columns: [\r\n        {\r\n            title: \"序号\", //序号\r\n            type: \"index\",\r\n            width: 60,\r\n            align: \"center\",\r\n        },\r\n        {  title: \"路由说明\", key: \"remark\", minWidth: 125},\r\n        {  title: \"路由号码\", key: \"numbers\", minWidth: 125},\r\n        {  title: \"路由目的\", key: \"routeType\", minWidth: 125},\r\n        {  title: \"优先级\", key: \"sortCode\", minWidth: 125},\r\n        {  title: \"转接开始时间\", key: \"transferStartTime\", minWidth: 125},\r\n        {  title: \"转接结束时间\", key: \"transferEndTime\", minWidth: 125},\r\n        {  title: \"转接路由号码\", key: \"routeNumbers\", minWidth: 125},\r\n        {\r\n            title: this.$t(\"operation\"), //操作\r\n            key: \"active\",\r\n            width: 100,\r\n            'checkeds':true, //自定义属性，默认选中，必须展示的数据\r\n            render: (h, params) => {\r\n                return h(\"div\", [\r\n                    //编辑\r\n                    h('Tooltip',{props: {\r\n                            placement: 'top',\r\n                            transfer: true\r\n                        }},[h(\r\n                        \"Icon\",\r\n                        {\r\n                            props: {\r\n                                type: \"ios-create-outline\",\r\n                                size: \"\",\r\n                            },\r\n                            style: { marginRight: \"5px\",fontSize:'18px',cursor: 'pointer' },\r\n                            on: {\r\n                                click: () => {\r\n                                    this.edit(params.row);\r\n                                },\r\n                            },\r\n                        },\r\n                        this.$t(\"play\")\r\n                    ),h('span', {\r\n                        slot: 'content',\r\n                        style: {\r\n                            whiteSpace: 'normal'\r\n                        }\r\n                    }, '编辑')]),\r\n\r\n\r\n                    //删除\r\n                    h('Tooltip',{props: {\r\n                            placement: 'top',\r\n                            transfer: true\r\n                        }},[h(\r\n                        \"Icon\",\r\n                        {\r\n                            props: {\r\n                                type: \"ios-trash-outline\",\r\n                                size: \"\",\r\n                            },\r\n                            style: { marginRight: \"5px\",fontSize:'18px',cursor: 'pointer' },\r\n                            on: {\r\n                                click: () => {\r\n                                    this.remove(params.row);\r\n                                },\r\n                            },\r\n                        },\r\n                        this.$t(\"play\")\r\n                    ),h('span', {\r\n                        slot: 'content',\r\n                        style: {\r\n                            whiteSpace: 'normal'\r\n                        }\r\n                    }, '删除')])\r\n                    ]);\r\n                }\r\n            }\r\n      ],\r\n      data: [],\r\n      total: 0,\r\n    };\r\n  },\r\n  methods: {\r\n    init() {\r\n      this.getSysRoutePageList();\r\n    },\r\n    changeSelect(e) {\r\n      this.selectList = e;\r\n      this.selectCount = e.length;\r\n      if(this.selectCount > 0){\r\n         this.openTip = true;\r\n      }else{\r\n         this.openTip = false;\r\n      }\r\n    },\r\n    handleSearch() {\r\n      this.selectList = \"\";\r\n      this.selectCount = 0;\r\n      this.searchForm.pagember = 1;\r\n      this.searchForm.pageSize = 10;\r\n      this.getSysRoutePageList();\r\n    },\r\n    handleReset() {\r\n      this.$refs.searchForm.resetFields();\r\n      this.searchForm.pageNumber = 1;\r\n      this.searchForm.pageSize = 10;\r\n    },\r\n    changePage(v) {\r\n      this.searchForm.pageNumber = v;\r\n      this.getSysRoutePageList();\r\n      this.clearSelectAll();\r\n    },\r\n    changePageSize(v) {\r\n      this.searchForm.pageSize = v;\r\n      this.getSysRoutePageList();\r\n    },\r\n    clearSelectAll() {\r\n      this.$refs.table.selectAll(false);\r\n    },\r\n    showSelect(e) {\r\n      this.selectList = e;\r\n      this.selectCount = e.length;\r\n    },\r\n    changeSort(e) {\r\n      this.searchForm.sort = e.key;\r\n      this.searchForm.order = e.order;\r\n      if (e.order == \"normal\") {\r\n        this.searchForm.order = \"\";\r\n      }\r\n      this.getSysRoutePageList();\r\n    },\r\n    getSysRoutePageList() {\r\n        this.loading = true;\r\n        var formdata={};\r\n        for(var i in this.searchForm){\r\n          if(this.searchForm[i] != null && this.searchForm[i] !=\"\"){\r\n            formdata[i] = this.searchForm[i]; \r\n          }\r\n        }\r\n        getSysRouteAllByPage(formdata).then(res => {\r\n          this.loading = false;\r\n          if (res.success) {\r\n            this.data = res.result.list;\r\n            this.total = res.result.total;\r\n          }\r\n        });\r\n    },\r\n    addSysRoute(){\r\n          this.modalTitle=\"新增路由规则\";\r\n          this.ModalVisible = true;\r\n          this.modalType = 0;\r\n          this.sysRouteForm ={};\r\n    },\r\n    edit(v) {\r\n      var formdata={};\r\n      for(let attr in v){\r\n        if(v[attr] != null && v[attr] !=\"\"){\r\n          formdata[attr] = v[attr]+\"\"; \r\n        }\r\n      }\r\n      this.modalType = 1;\r\n      this.ModalVisible = true;\r\n      this.modalTitle = \"编辑路由规则\";\r\n      this.sysRouteForm = formdata;\r\n    },\r\n    submitSysRoute() {\r\n          var formdata={};\r\n          if(!this.sysRouteForm.sortCode) {\r\n            this.$Message.warning(\"优先级不能为空!\");\r\n            return;\r\n          }\r\n          for(var i in this.sysRouteForm){\r\n            if(this.sysRouteForm[i] != null && this.sysRouteForm[i] !=\"\"){\r\n                 formdata[i] = this.sysRouteForm[i];  \r\n            }\r\n          }\r\n          if (this.modalType == 0) {\r\n            // 添加\r\n            this.submitLoading = true;\r\n            addSysRoute(formdata).then(res => {\r\n              this.submitLoading = false;\r\n              if (res.success) {\r\n                this.$Message.success(\"操作成功\");\r\n                this.getSysRoutePageList();\r\n                this.ModalVisible = false;\r\n              }\r\n            });\r\n          } else {\r\n            this.submitLoading = true;\r\n            editSysRoute(formdata).then(res => {\r\n              this.submitLoading = false;\r\n              if (res.success) {\r\n                this.$Message.success(\"操作成功\");\r\n                this.getSysRoutePageList();\r\n                this.ModalVisible = false;\r\n              }\r\n            });\r\n        }\r\n    },\r\n    remove(v) {\r\n      this.$Modal.confirm({\r\n        title: \"确认删除\",\r\n        // 记得确认修改此处\r\n        content: \"您确认要删除 \" + v.numbers + \" ?\",\r\n        loading: true,\r\n        onOk: () => {\r\n          // 删除\r\n          deleteSysRoute({ ids: v.id }).then((res) => {\r\n            this.$Modal.remove();\r\n            if (res.success) {\r\n              this.$Message.success(\"操作成功\");\r\n              this.getSysRoutePageList();\r\n              this.selectCount = 0;\r\n            }\r\n          });\r\n        },\r\n      });\r\n    },\r\n    cancelSysRoute() {\r\n      this.ModalVisible = false;\r\n    },\r\n    getRelayPhoneAll() { //获取当前公司中继号码\r\n      getRelayPhoneList({blackMark:1,workMode:1}).then((res) => {\r\n        if (res.success) {\r\n          this.relayPhoneListItems = [];\r\n          this.relayPhoneListItems = res.result.list;\r\n        }\r\n      });\r\n    },\r\n    getIvrRobotAll() { //获取机器人信息\r\n      getAllByIvrRobot({}).then((res) => {\r\n        if (res.success) {\r\n          this.robotListItems = [];\r\n          this.robotListItems = res.result.list;\r\n        }\r\n      });\r\n    }\r\n\r\n    \r\n  },\r\n  mounted() {\r\n    // 计算高度\r\n    this.height = Number(document.documentElement.clientHeight - 230);\r\n    this.init();\r\n  }\r\n};\r\n</script>"]}]}